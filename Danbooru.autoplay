<Event>
<Name>On Select</Name>
<Args/>
<Script>local Selected = ThumbList.GetSelectedItems(this);

if (Selected) then
	xButton.SetEnabled("Item_delete", true);
	xButton.SetEnabled("Item_edit", true);
	
	Danbooru.Library.Selected = {};
	
	for K, V in pairs(Selected) do
		local ThumbnailFile = ThumbList.GetItemInfo(this, V).filename;
		
		if (ThumbnailFile) then
			local DataTable = nil;
			
			for K, V in pairs(Danbooru.Library.Posts) do
				if (V.Preview.Filename == ThumbnailFile) then
					DataTable = V;
					break;
				end
			end
			
			if not (DataTable) then
				Dialog.Message("Danbooru", "Failed to get data table from thumbnail filename. ("..K.."/"..#Selected..")", 0, 48, 0);
			else
				table.insert(Danbooru.Library.Selected, DataTable);
			end
		end
	end
	
	local SelectedCount = Table.Count(Danbooru.Library.Selected);
	
	if (SelectedCount == 0) then
		Paragraph.SetText("Paragraph3", "No item selected.");
	elseif (SelectedCount == 1) then
		pcall(Paragraph.SetText, "Paragraph3", Table.Concat(Danbooru.Library.Selected[1].Tags, " ", 1, -1));
	else
		Paragraph.SetText("Paragraph3", SelectedCount.." items selected.");
	end
else
	xButton.SetEnabled("Item_delete", false);
	xButton.SetEnabled("Item_edit", false);
end</Script>

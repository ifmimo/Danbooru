<Args/>
<Script>Danbooru.Library.Pages = {{}};

ThumbList.Clear("Thumbnails");
Paragraph.SetText("Paragraph3", "No item selected.");

-- Default: 25
local Count = 25;
		
if (RadioButton.GetChecked("RadioButton2")) then
	Count = 50;
elseif (RadioButton.GetChecked("RadioButton3")) then
	Count = 100;
end

if (Danbooru.Library.CensoredPosts) then
	for K, V in pairs(Danbooru.Library.CensoredPosts) do
		local CurrentPage = Danbooru.Library.Pages[#Danbooru.Library.Pages];
		
		if (#CurrentPage == Count) then
			Danbooru.Library.Pages[#Danbooru.Library.Pages + 1] = {};
			CurrentPage = Danbooru.Library.Pages[#Danbooru.Library.Pages];
		end
		
		table.insert(CurrentPage, Danbooru.LibraryPath.."\\.thumbnails\\"..V.Preview.Filename);
	end
end

Label.SetText("#Pagenumber", "1")
ThumbList.AddFiles("Thumbnails", Danbooru.Library.Pages[1]);
Paragraph.SetText("Paragraph2", "Page 1 of "..Table.Count(Danbooru.Library.Pages).." - Total: "..Table.Count(Danbooru.Library.CensoredPosts).." files");</Script>

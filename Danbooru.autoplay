<Event>
<Name>On Show</Name>
<Args/>
<Script>-- Create ListIcon object.
Danbooru.SelectionListIcon = ListIconEx.CreateObject("ImageboardsListbox", "Imageboard", 130, ListIconEx.GenerateFlagsTable(true, false, true, true, false, true));

-- Add column.
Danbooru.SelectionListIcon:AddColumn("URL", 200);

-- Add data.
for K, V in pairs(Danbooru.Sources) do
	Danbooru.SelectionListIcon:AddRow(K..NEXT_CELL..Danbooru.API[K].Main, Danbooru.Icons[K]);
end

-- Check rows.
for K, V in pairs(Danbooru.Sources) do
	for I = 0, (Danbooru.SelectionListIcon:CountRows() - 1) do
		if (K == Danbooru.SelectionListIcon:GetCellText(I, 0)) then
			Danbooru.SelectionListIcon:SetRowChecked(I, (tostring(V) == "true"));
		end
	end
end</Script>

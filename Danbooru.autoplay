<Event>
<Name>On Menu</Name>
<Args>number e_ID, table e_ItemInfo</Args>
<Script>-- Menu handler for item "&amp;Exit Danbooru.."
if (e_ID == 5) then
	-- This will trigger the close hook to ensure downloads finish.
	Application.Exit(0);
end

-- Menu handler for item "&amp;About Danbooru.."
if (e_ID == 4) then
	DialogEx.Show("About");
end

-- Menu handler for item "&amp;Perform search.."
if (e_ID == 6) then
	Page.ClickObject("xButton1");
end

-- Menu handler for item "&amp;Clear post list.."
if (e_ID == 7) then
	Page.ClickObject("xButton2");
end

-- Menu handler for item "&amp;View pools.."
if (e_ID == 10) then
	if (__DOWNLOAD) then
		Dialog.Message("Danbooru", "Please wait for your download to complete first.", 0, 48, 0);
	else
		Page.Jump("com.danbooru.pools");
	end
end

-- Menu handler for item "Check &amp;all.."
if (e_ID == 11) then
	Danbooru.ListIcon.AllowPerformHook = false;
	Danbooru.ListIcon:CheckAllRows(true);
	Danbooru.ListIcon.AllowPerformHook = true;
	Danbooru.ListIcon:SetRowSelected(0, true);
	Danbooru.UpdateDownloadQueue();
end

-- Menu handler for item "&amp;Check none.."
if (e_ID == 13) then
	Danbooru.ListIcon.AllowPerformHook = false;
	Danbooru.ListIcon:CheckAllRows(false);
	Danbooru.ListIcon.AllowPerformHook = true;
	Danbooru.UpdateDownloadQueue();
end

-- Menu handler for item "&amp;Inverse checked items.."
if (e_ID == 14) then
	Danbooru.ListIcon.AllowPerformHook = false;
	Danbooru.ListIcon:InverseChecked();
	Danbooru.ListIcon.AllowPerformHook = true;
	Danbooru.ListIcon:SetRowSelected(0, true);
	Danbooru.UpdateDownloadQueue();
end

-- Menu handler for item "&amp;Configure Danbooru.."
if (e_ID == 9) then
	if (__DOWNLOAD) then
		Dialog.Message("Danbooru", "Please wait for your download to complete first.", 0, 48, 0);
	else
		DialogEx.Show("Settings");
	end
end</Script>

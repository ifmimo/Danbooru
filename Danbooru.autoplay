<Script>local SFWFilter = RadioButton.GetChecked("RadioButton1");
local QSFWFilter = RadioButton.GetChecked("RadioButton2");
local NoFilter = RadioButton.GetChecked("RadioButton3");

if (SFWFilter) then
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", false);
	Danbooru.SetSettingsValue("AllowExplicitImages", false);
elseif (QSFWFilter) then
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", true);
	Danbooru.SetSettingsValue("AllowExplicitImages", false);
else
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", true);
	Danbooru.SetSettingsValue("AllowExplicitImages", true);
end

Danbooru.SetSettingsValue("RememberSourceSelection", CheckBox.GetChecked("CheckBox1"));
Danbooru.SetSettingsValue("UseGelbooruWorkaround", CheckBox.GetChecked("CheckBox2"));
Danbooru.SetSettingsValue("ShowSplashScreen", CheckBox.GetChecked("CheckBox3"));
Danbooru.SetSettingsValue("ClearListOnNewSearch", CheckBox.GetChecked("CheckBox4"));
Danbooru.SetSettingsValue("InteractionPlaySound", CheckBox.GetChecked("CheckBox5"));
Danbooru.SetSettingsValue("NoReplacePrompt", CheckBox.GetChecked("CheckBox6"));
Danbooru.SetSettingsValue("NeverGetNotes", CheckBox.GetChecked("CheckBox7"));
Danbooru.SetSettingsValue("RememberTags", CheckBox.GetChecked("CheckBox8"));
Danbooru.SetSettingsValue("RememberPageAndLimit", CheckBox.GetChecked("CheckBox9"));
Danbooru.SetSettingsValue("UseURLHandler", CheckBox.GetChecked("CheckBox10"));

Danbooru.SetSettingsValue("EnableNewColorRequest", RadioButton.GetChecked("RadioButton4"));
Danbooru.SetSettingsValue("EnableNewStatusColor", RadioButton.GetChecked("RadioButton5"));

if (CheckBox.GetChecked("CheckBox6")) and (ComboBox.GetItemText("ComboBox1", ComboBox.GetSelected("ComboBox1")) == "overwrite") then
	Danbooru.SetSettingsValue("NoReplaceType", "overwrite");
else
	Danbooru.SetSettingsValue("NoReplaceType", "keep");
end

Danbooru.SetSettingsValue("Blacklist", Input.GetText("Input2"));

Danbooru.WriteSettingsFile();

DialogEx.Close(0);</Script>

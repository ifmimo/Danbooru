<Args/>
<Script>-- Collect information and validate.
local Tags = Input.GetText("Input1");

if (String.Replace(Tags, " ", "", false) == "") then
	return Dialog.Message("Danbooru", "You must specifiy at least one tag.", 0, 48, 0);
end

local Safe, Questionable, Explicit = RadioButton.GetChecked("RadioButton1"), RadioButton.GetChecked("RadioButton2"), RadioButton.GetChecked("RadioButton3");

local Author = Input.GetText("Input2");

-- Set the new data and commit back to My Library.
local DataTable = DialogValues.GetValue("TagEditor", "DataTable");

DataTable.Tags = String.Explode(" ", Tags);
DataTable.Rating.Safe, DataTable.Rating.Questionable, DataTable.Rating.Explicit = Safe, Questionable, Explicit;
DataTable.Author = Author;

DialogValues.SetValue("TagEditor", "DataTable", DataTable);

DialogEx.Close(0);</Script>

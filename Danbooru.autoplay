<Script>local FilesToImport = Dialog.FileBrowse(true, "Select files to import..", Danbooru.LibraryPath, "Image Files (*.*)|*.*|", "", "", true, true);

if (FilesToImport[1] ~= "CANCEL") then
	SourceTables = {};
	
	StatusDlg.Show(64, false);
	
	for K, V in pairs(FilesToImport) do
		if not (File.DoesExist(Danbooru.LibraryPath.."\\"..String.SplitPath(V).Filename..String.SplitPath(V).Extension)) then
			File.Copy(V, Danbooru.LibraryPath.."\\", false, true, false, true, nil);
		end
		
		if (Application.GetLastError() == 0) then
			local Index = #SourceTables + 1;
			local nSize = File.GetSize(V);
			local tImageInfo = Image.GetFileInfo(V);
			
			SourceTables[Index] = {};
			SourceTables[Index].Tags = {"tagme", "imported"};
			SourceTables[Index].Deleted = false;
			SourceTables[Index].Source = "imported";
			SourceTables[Index].MD5 = Crypto.MD5DigestFromFile(V);
			SourceTables[Index].FormattedSize = String.GetFormattedSize(File.GetSize(nSize), 1, true);
			SourceTables[Index].Filename = String.SplitPath(V).Filename..String.SplitPath(V).Extension;
			SourceTables[Index].ParentID = 0;
			SourceTables[Index].ID = K;
			SourceTables[Index].Rating = {String = "Questionable", Safe = false, Questionable = true, Explicit = false};
			SourceTables[Index].Size = nSize;
			SourceTables[Index].Status = "active";
			SourceTables[Index].Pending = false;
			SourceTables[Index].Active = true;
			SourceTables[Index].HasComments = false;
			SourceTables[Index].Preview = {Filename = String.SplitPath(V).Filename..".png"}
			SourceTables[Index].URL = "http://danbooru.donmai.us";
			SourceTables[Index].Width = tImageInfo.Width;
			SourceTables[Index].Height = tImageInfo.Height;
			SourceTables[Index].HasChildren = false;
			SourceTables[Index].Sample = {};
			SourceTables[Index].Score = 0;
			SourceTables[Index].CreatorID = 0;
			SourceTables[Index].Author = System.GetUserInfo().RegOwner;
			SourceTables[Index].HasNotes = false;
		end
	end
	
	StatusDlg.Hide();
	
	if (#SourceTables ~= 0) then
		Danbooru.InsertPost(SourceTables, true);
	
		if (DialogEx.Show("Thumbify") == 0) then
			Page.ClickObject("xButton4");
		end
	end
end</Script>

<Event>
<Name>On Show</Name>
<Args/>
<Script>Progress.SetText("Progress1", "Searching..");

local NewFiles = DialogValues.GetValue("Scriptify", "Files");

if (NewFiles) then
	DialogValues.TruncateDialogTable("Scriptify");
	
	if not (Folder.DoesExist(Danbooru.LibraryPath.."\\.thumbnails")) then
		Folder.Create(Danbooru.LibraryPath.."\\.thumbnails");
	end
	
	Progress.SetRange("Progress1", 0, #NewFiles);
	
	for K, V in pairs(NewFiles) do
		Progress.SetCurrentPos("Progress1", K);
		
		local Filename = String.SplitPath(V).Filename;
		local Extension = String.SplitPath(V).Extension;
		
		if (Extension == ".png" or Extension == ".jpeg" or Extension == ".jpg") then
			if not (File.DoesExist(Danbooru.LibraryPath.."\\.thumbnails\\"..Filename..".png")) then
				Progress.SetText("Progress1", "Generating..");
				
				Label.SetText("Label4", Filename..Extension);
				
				if (NewFiles[K + 1]) then
					local NextSplit = String.SplitPath(NewFiles[K + 1]);
					Label.SetText("Label5", NextSplit.Filename..NextSplit.Extension);
				else
					Label.SetText("Label5", "N/A");
				end
				
				if (Thumbify.Scaled(V, Danbooru.LibraryPath.."\\.thumbnails\\"..Filename..".png", Danbooru.ThumbnailWidth, Danbooru.ThumbnailHeight) &lt; 0) then
					Progress.SetText("Progress1", Filename.." FAILED!..");
					Application.Sleep(500);
				else
					Label.SetText("Label1", Filename);
					Image.Load("Image1", Danbooru.LibraryPath.."\\.thumbnails\\"..Filename..".png");
				end
			end
		end
	end
end

Image.Load("Image1", "AutoPlay\\Images\\blank.png");
DialogEx.Close(0);</Script>

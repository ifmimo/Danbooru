<Event>
<Name>On Click</Name>
<Args/>
<Script>-- Get the checked objects.
if (Danbooru.SelectionListIcon) then
	local Checked = Danbooru.SelectionListIcon:GetCheckedRows();
	
	if (Checked) then
		-- Set all currently selected boards to false.
		for K, V in pairs(Danbooru.Sources) do
			Danbooru.Sources[K] = false;
		end
		
		-- Get checked boards and set selected status.
		for K, V in pairs(Checked) do
			local sImageboardName = Danbooru.SelectionListIcon:GetCellText(V, 0);
			Danbooru.Sources[sImageboardName] = true;
		end
	end
end

DialogEx.Close(0);</Script>

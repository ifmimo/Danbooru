<Event>
<Name>On Click</Name>
<Args/>
<Script>local Items = Danbooru.Library.Selected;

if (Items) then
	if (Dialog.Message("Danbooru", "Are you sure you would like to delete the selected images?", 4, 32, 256) == IDYES) then
		for K, V in pairs(Items) do
			-- Delete the thumbnail.
			File.Delete(Danbooru.LibraryPath.."\\.thumbnails\\"..V.Preview.Filename, false, false, false, nil);
			
			-- Delete the real image.
			File.Delete(Danbooru.LibraryPath.."\\"..V.Filename, false, false, false, nil);
		end
		
		Danbooru.ReloadDatabase();
		
		Paragraph.SetText("Paragraph3", "No item selected.");
		ThumbList.Clear("Thumbnails");
		
		local Count = 25;

		if (RadioButton.GetChecked("RadioButton2")) then
			Count = 50;
		elseif (RadioButton.GetChecked("RadioButton3")) then
			Count = 100;
		end
		
		for K, V in pairs(Danbooru.Library.Posts) do
			local CurrentPage = Danbooru.Library.Pages[#Danbooru.Library.Pages];
			
			if (#CurrentPage == Count) then
				Danbooru.Library.Pages[#Danbooru.Library.Pages + 1] = {};
				CurrentPage = Danbooru.Library.Pages[#Danbooru.Library.Pages];
			end
			
			table.insert(CurrentPage, Danbooru.LibraryPath.."\\.thumbnails\\"..V.Preview.Filename);
		end
		
		Label.SetText("#Pagenumber", "1");
		ThumbList.AddFiles("Thumbnails", Danbooru.Library.Pages[1]);
		Paragraph.SetText("Paragraph2", "Page 1 of "..Table.Count(Danbooru.Library.Pages).." - Total: "..Table.Count(Danbooru.Library.Posts).." files");
	end
end</Script>

-- Get download queue size, etc.
function Danbooru.UpdateDownloadQueue()
	local FileSize = 0;
	local FormattedSize = "0 KB";
	
	local Checked = Danbooru.ListIcon:GetCheckedRows() or {};
	
	if (Checked) and (Table.Count(Checked) &gt;= 1) then
		for K, V in pairs(Checked) do
			if (Danbooru.Posts[V + 1]) then			
				FileSize = FileSize + Danbooru.Posts[V + 1].Size;
			end
		end
	end
	
	FormattedSize = String.GetFormattedSize(FileSize, 1, true);
	
	local function s(n) if (n == 1) then return "" else return "s" end end
	
	Paragraph.SetText("Paragraph3", "("..#Checked.." item"..s(#Checked).." in download queue - "..FormattedSize..")");
end

-- Query post API, then insert into ListIcon.
function Danbooru.QueryAndInsert(API, Icon, Query, MaxTags)
	if (MaxTags) then
		if (Table.Count((String.Explode(" ", Query[1]) or {})) &gt; MaxTags) then
			Dialog.Message("Error", API.Name.." restricts users to a maximum of "..MaxTags.." tags for search queries.", 0, 48, 0);
			return false;
		end
	end
	
	local Results, Error = Danbooru.Post(Query[1], Query[2], Query[3], API);
	
	if (Results == false) then
		if (API.Type ~= "XML") then
			Dialog.Message("Error", "An error occured while querying the "..API.Name.." API.\r\n\r\n"..tostring(Error), 0, 48, 0);
		end
	else
		for K, V in pairs(Results) do
			local BuiltString = "";
			BuiltString = BuiltString..V.ID or "Unknown";
			BuiltString = BuiltString..NEXT_CELL..Table.Concat(V.Tags or {}, " ", 1, -1);
			BuiltString = BuiltString..NEXT_CELL..V.Width or "0";
			BuiltString = BuiltString.."x"..V.Height or "0";
			BuiltString = BuiltString..NEXT_CELL..tostring((V.FormattedSize or "")) or "Unknown";
			BuiltString = BuiltString..NEXT_CELL..V.Rating.String or "Unknown";
			
			Danbooru.ListIcon:AddRow(BuiltString, Icon);
		end
	end
end

-- Just insert, no query.
function Danbooru.JustInsert(ResultSet, Icon)
	for K, V in pairs(ResultSet) do
		local BuiltString = "";
		BuiltString = BuiltString..V.ID or "Unknown";
		BuiltString = BuiltString..NEXT_CELL..Table.Concat(V.Tags or {}, " ", 1, -1);
		BuiltString = BuiltString..NEXT_CELL..V.Width or "0";
		BuiltString = BuiltString.."x"..V.Height or "0";
		BuiltString = BuiltString..NEXT_CELL..tostring((V.FormattedSize or "")) or "Unknown";
		BuiltString = BuiltString..NEXT_CELL..V.Rating.String or "Unknown";
		
		table.insert(Danbooru.Posts, V);
		Danbooru.ListIcon:AddRow(BuiltString, Icon);
	end
end


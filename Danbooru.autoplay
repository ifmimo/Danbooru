<Event>
<Name>On Click</Name>
<Args/>
<Script>local Count = Table.Count(Danbooru.Blacklisted);

if (Count == 0) then
	Dialog.Message("Danbooru", "No blacklisted posts to show.", 0, 48, 0);
else
	if (Dialog.Message("Danbooru", "Are you sure you would like to show "..Count.." blacklisted posts?", 4, 48, 0) == IDYES) then
		Danbooru.JustInsert(Danbooru.Blacklisted, Danbooru.Icons.Blacklisted);
		Danbooru.Blacklisted = {};
		
		local function s(n) if (n == 1) then return "" else return "s" end end
		
		local i_intCount = Danbooru.ListIcon:CountRows();
		
		Paragraph.SetText("Paragraph2", "("..tostring(i_intCount).." item"..s(i_intCount)..", 0 blacklisted)");
	end
end</Script>

<Event>
<Name>On Click</Name>
<Args/>
<Script>if (__DOWNLOAD) then
	Dialog.Message("Danbooru", "Please wait for your download to complete first.", 0, 48, 0);
else
	xButton.SetEnabled("xButton1", false);
	xButton.SetEnabled("xButton2", false);
	xButton.SetEnabled("xButton6", false);
	
	-- We don't care about string data validation, if the search query is blank it returns all posts.
	local Query = Input.GetText("Input1");
	
	if (String.Replace(Query, " ", "", false) == "") then
		Query = "";
	end
	
	if (tostring(Danbooru.Settings.RememberTags) == "true") then
		Danbooru.Settings.LastQuery = tostring(Query);
		Danbooru.WriteSettingsFile();
	end
	
	-- If no imageboards are selected, we do NOTHING.
	local Proceed = false;
	
	for K, V in pairs(Danbooru.Sources) do
		if (tostring(V) == "true") then
			Proceed = true;
			break;
		end
	end
	
	if not (Proceed) then
		if (Dialog.Message("Danbooru", "No imageboards are currently selected.\r\n\r\nWould you like to open the imageboard selection dialog?", 4, 32, 0) == IDYES) then
			Page.ClickObject("xButton6");
			
			local Proceed = false;
	
			for K, V in pairs(Danbooru.Sources) do
				if (tostring(V) == "true") then
					Proceed = true;
					break;
				end
			end
			
			if (Proceed == false) then
				return;
			end
		else
			return;
		end
	end
	
	-- Check if it's over 100.
	if (String.ToNumber(Input.GetText("Input3")) &gt; 1000) then
		return Dialog.Message("Danbooru", "The maximum limit is 1000.", 0, 16, 0);
	end
	
	-- Clear the list if the user selected it in the settings dialog.
	if (tostring(Danbooru.Settings.ClearListOnNewSearch) == "true") then
		Danbooru.EmptyListIcon();
	end
	
	-- Insert time.
	local Query = {Input.GetText("Input1"), String.ToNumber(Input.GetText("Input3")), String.ToNumber(Input.GetText("Input2"))}; -- no need to recreate the table everytime.
	
	-- Table for query times.
	local t_tblQueryTimes = {};
	
	for K, V in pairs(Danbooru.Sources) do
		if ((V == true) and (Danbooru.API[K]) and (Danbooru.Icons[K])) then
			Paragraph.SetText("Paragraph1", "Querying "..Danbooru.API[K].Name.."..");
			Danbooru.QueryAndInsert(Danbooru.API[K], Danbooru.Icons[K], Query, Danbooru.API[K].MaxTags);
		end
	end
	
	local function s(n) if (n == 1) then return "" else return "s" end end
	
	local i_intCount = Danbooru.ListIcon:CountRows();
	
	Paragraph.SetText("Paragraph2", "("..tostring(i_intCount).." item"..s(i_intCount)..", "..Table.Count(Danbooru.Blacklisted).." blacklisted)");
	Paragraph.SetText("Paragraph1", "Idle.");
	
	xButton.SetEnabled("xButton1", true);
	xButton.SetEnabled("xButton2", true);
	xButton.SetEnabled("xButton6", true);
end</Script>

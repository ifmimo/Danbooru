<Event>
<Name>On Show</Name>
<Args/>
<Script>local Data = DialogValues.GetValue("Preview", "Data");

if (Data) then	
	if (Data.URL:sub(Data.URL:len() - 3, -1) == ".swf") then
		Window.SetText(DialogEx.GetWndHandle(), "Viewing flash..");
		local HTMLCode = [[&lt;html&gt;
		&lt;head&gt;
			&lt;style type="text/css"&gt;
				* { padding: 0; margin: 0; }
				body { background: #000000; width: 798px; height: 798px; }
				img { vertical-align: middle; }
			&lt;/style&gt;
		&lt;/head&gt;
		&lt;body&gt;
			&lt;div id="content"&gt;
				&lt;object style="width:100%; height:100%;"&gt;
				&lt;param name="movie" value="]]..Data.URL..[["&gt;
					&lt;embed src="]]..Data.URL..[[" style="width:100%; height:100%;"&gt;
					&lt;/embed&gt;
				&lt;/object&gt;
			&lt;/div&gt;
		&lt;/body&gt;
	&lt;/html&gt;]];
		Web.LoadHTML("Web1", HTMLCode);
	else
		local Width, Height = Data.Preview.Width, Data.Preview.Height;
		
		repeat
			Width = Width * 1.1;
			Height = Height * 1.1;
		until (Width &gt;= 800 and Height &gt;= 600)
		
		local HTMLCode = [[&lt;html&gt;
		&lt;head&gt;
			&lt;style type="text/css"&gt;
				* { padding: 0; margin: 0; }
				body { background: #000000; width: 798px; height: 598px; }
				img { vertical-align: middle; }
			&lt;/style&gt;
			&lt;script type="text/javascript"&gt;
			&lt;!--
			var flag = true;
			function resize() {
				if(flag) {
					document.getElementById("img1").style.width = "]]..Width..[[px";
					document.getElementById("img1").style.height = "]]..Height..[[px";
				} else {
					document.getElementById("img1").style.width = "100%";
					document.getElementById("img1").style.height = "100%";
				}
				(flag)?flag=false:flag=true;
			} 
			//--&gt;
			&lt;/script&gt;
		&lt;/head&gt;
		&lt;body&gt;
			&lt;div id="content"&gt;
				&lt;img src="]]..Data.Sample.URL..[[" border="0" id="img1" onclick="resize();" /&gt;
			&lt;/div&gt;
		&lt;/body&gt;
	&lt;/html&gt;]];
		Web.LoadHTML("Web1", HTMLCode);
	end
end

Window.SetText(DialogEx.GetWndHandle(), "Previewing image.. (click to resize / adjust window and press F5)");</Script>

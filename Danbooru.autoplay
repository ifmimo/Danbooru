<Script>local CurrentPage = String.ToNumber(Label.GetText("#Pagenumber"));
local TargetPage = #Danbooru.Library.Pages;

local ItemsPerPage = 25;

if (RadioButton.GetChecked("RadioButton2")) then
	ItemsPerPage = 50;
elseif (RadioButton.GetChecked("RadioButton3")) then

<Event>
<Name>On Click</Name>
<Args/>
<Script>if (__DOWNLOAD) then
	Dialog.Message("Danbooru", "Please wait for your download to complete first.", 0, 48, 0);
else
	-- We don't care about string data validation, if the search query is blank it returns all posts.
	local Query = Input.GetText("Input1");
	
	if (String.Replace(Query, " ", "", false) == "") then
		Query = "";
	end
	
	-- If no imageboards are selected, we do NOTHING.
	local Proceed = false;
	
	for K, V in pairs(Danbooru.Sources) do
		if (tostring(V) == "true") then
			Proceed = true;
			break;
		end
	end
	
	if not (Proceed) then
		if (Dialog.Message("Danbooru", "No imageboards are currently selected.\r\n\r\nWould you like to open the imageboard selection dialog?", 4, 32, 0) == IDYES) then
			Page.ClickObject("xButton6");
			
			local Proceed = false;
	
			for K, V in pairs(Danbooru.Sources) do
				if (tostring(V) == "true") then
					Proceed = true;
					break;
				end
			end
			
			if (Proceed == false) then
				return;
			end
		else
			return;
		end
	end
	
	ListBoxEx.DeleteAllItems("Plugin2");
	
	-- Insert time.
	for K, V in pairs(Danbooru.Sources) do
		if ((V == true) and (Danbooru.API[K])) then
			local Result = Danbooru.Pool(Query, String.ToNumber(Input.GetText("Input2")), Danbooru.API[K]);
			
			local IconID = 0;
			
			if (K == "Danbooru") then
				IconID = 1;
			elseif (K == "Konachan") then
				IconID = 2;
			elseif (K == "MoeImouto") then
				IconID = 3;
			elseif (K == "Sankaku") then
				IconID = 4;
			elseif (K == "Nekobooru") then
				IconID = 5;
			end
			
			if (type(Result) == "table") then
				for X, Y in pairs(Result) do
					if (type(Y) == "table") then
						if (tostring(Y.Name) ~= "nil") and (tostring(Y.ID) ~= "nil" or tostring(Y.ID) == "0") then
							ListBoxEx.AddItem("Plugin2", "&lt;font face=\"Tahoma\" size=\"14\"&gt;&lt;b&gt;"..tostring(Y.Name).."&lt;/b&gt;&amp;nbsp;&lt;font size=\"12\" color=\"#393939\"&gt;("..tostring(Y.PostCount).." posts)&lt;/font&gt;&lt;br&gt;Last Updated: "..Y.UpdatedAt.."&lt;/font&gt;", "", tostring(Y.ID), K, IconID, LBXITEM_HTML, 0);
						end
					end
				end
			elseif (type(Result) == "boolean") then
				Dialog.Message("Danbooru", "Failed to get pools from "..K.."!", 0, 48, 0);
			end
		end
	end
	
	Paragraph.SetText("Paragraph1", "Idle.");
end</Script>

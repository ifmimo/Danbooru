<Event>
<Name>On Double-Click</Name>
<Args>number e_Item</Args>
<Script>local ThumbnailFile = ThumbList.GetItemInfo(this, e_Item).filename;

if (ThumbnailFile) then
	local DataTable = nil;
	
	for K, V in pairs(Danbooru.Library.Posts) do
		if (V.Preview.Filename == ThumbnailFile) then
			DataTable = V;
			break;
		end
	end
	
	if not (DataTable) then
		Dialog.Message("Danbooru", "Failed to get data table from thumbnail filename.", 0, 48, 0);
	else
		if (DataTable.HasNotes) then
			if (Dialog.Message("Danbooru", "Would you like to view this image with notes enabled?", 4, 32, 0) == 6) then
				DialogValues.SetValue("Preview", "Data", DataTable);
				DialogEx.Show("NotesPreview");
			else
				Shell.Execute(Danbooru.LibraryPath.."\\"..DataTable.Filename, "open", "", Danbooru.LibraryPath, 1, false);
			end
		else
			Shell.Execute(Danbooru.LibraryPath.."\\"..DataTable.Filename, "open", "", Danbooru.LibraryPath, 1, false);
		end
	end
end</Script>

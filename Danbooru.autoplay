function ListIconEx:SetName(szName)
	self.Name = szName;
end

function ListIconEx:GetName()
	return self.Name;
end

function ListIconEx:GetSelectedRows()
	local Return = {};
	
	for I = 0, self:CountRows() do
		if (self:GetRowSelected(I)) then
			table.insert(Return, I);
		end
	end
	
	return Return;
end

function ListIconEx:GetCheckedRows()
	local Return = {};
	
	for I = 0, self:CountRows() do
		if (self:GetRowChecked(I)) then
			table.insert(Return, I);
		end
	end
	
	return Return;
end

function ListIconEx:CheckAllRows(ShouldCheck)
	for I = 0, self:CountRows() do
		self:SetRowChecked(I, ShouldCheck);
	end
end

function ListIconEx:SelectAllRows(ShouldCheck)
	for I = 0, self:CountRows() do
		self:SetRowSelected(I, ShouldCheck);
	end
end

function ListIconEx:GetObjectID()
	return self.ID;
end

function ListIconEx:InverseChecked()
	local Rows = self:CountRows();
	
	for I = 0, Rows do
		self:SetRowChecked(I, not self:GetRowChecked(I));
	end
end

function ListIconEx:InverseSelection()
	local Rows = self:CountRows();
	
	if (type(Rows) == "table") then
		for I = 0, Rows do
			self:SetRowSelected(I, not self:GetRowSelected(I));
		end
	end
end

-- Get people who favorited a post.
function Danbooru.GetFavoritedByID(ID, API)
	-- HALT! This doesn't work on Gelbooru.
	if (API.Type == "XML") then
		return Dialog.Message("Danbooru", "This feature is not supported in the Gelbooru API.", 0, 48, 0);
	end
	
	-- I'd add this to Danbooru.Post but it'll make it a lot slower.
	assert(type(ID) == "number", "Argument 1 must be of type number.");
	assert(type(API) == "table" and (API.Post and API.Notes), "Argument 1 must be a valid API.");
	
	Paragraph.SetText("Paragraph1", "Getting list of users who favorited post "..ID.."..");
	
	local Return = HTTP.Submit(API.FavoriteList, {id = ID}, 1, 15, 80);
	
	if (Return and Application.GetLastError() == 0) then
		Paragraph.SetText("Paragraph1", "Idle.");
		
		local Success, Users = pcall(JSON.Decode, Return);
		
		if (Success) then
			if (Users and type(Users.favorited_users) == "string") then
				return String.Explode(",", Users.favorited_users);
			end
		end
	end
end

-- Get pool listing. (Using xml is more reliable than JSON. D:)
function Danbooru.Pool(Tags, Page, API)
	assert(type(Tags) == "string", "Argument 1 must be of type string.");
	assert(type(Page) == "number", "Argument 2 must be of type number.");
	assert(type(API) == "table", "Argument 3 must be a valid API.");
	
	local Return;
	
	Paragraph.SetText("Paragraph1", "Querying "..API.Name.."..");
	
	if (API.Pool) then
		Return = HTTP.Submit(API.Pool, {query = Tags, page = Page}, 1, 10, 80);
	else
		return Dialog.Message("Danbooru", API.Name.." does not support this API method.", 0, 16, 0);
	end
	
	if (Return and Application.GetLastError() == 0) then
		Paragraph.SetText("Paragraph1", "Query executed successfully.");
		
		XML.SetXML(Return);
		
		if (Application.GetLastError() ~= 0) then
			Dialog.Message("Danbooru", "Parse error. (".._tblErrorMessages[Application.GetLastError()]..")", 0, 48, 0);
			return;
		end
		
		local ReturnTable = {};
		
		for K, V in pairs(XML.GetElementNames("pools", true, true) or {}) do
			local Index = Table.Count(ReturnTable) + 1;
			ReturnTable[Index] = {};
			
			ReturnTable[Index].ID = String.ToNumber(XML.GetAttribute(V, "id"));
			ReturnTable[Index].Name = String.Replace(XML.GetAttribute(V, "name"), "_", " ", false);
			ReturnTable[Index].CreatedAt = XML.GetAttribute(V, "created_at");
			ReturnTable[Index].UpdatedAt = XML.GetAttribute(V, "updated_at");
			ReturnTable[Index].UserID = String.ToNumber(XML.GetAttribute(V, "user_id"));
			ReturnTable[Index].PostCount = String.ToNumber(XML.GetAttribute(V, "post_count"));
			ReturnTable[Index].IsPublic = (XML.GetAttribute(V, "is_public") == "true");
		end
		
		return ReturnTable;
	end
end

-- Get posts, this is awesome.
function Danbooru.Post(Tags, Limit, Page, API)
	assert(type(Tags) == "string", "Argument 1 must be of type string.");
	assert(type(Limit) == "number", "Argument 2 must be of type number.");
	assert(type(Page) == "number", "Argument 3 must be of type number.");
	assert(type(API) == "table", "Argument 4 must be a valid API.");
	
	local Return;
	
	if (API.Type == "JSON") then
		Return = HTTP.Submit(API.Post, {tags = Tags, limit = Limit, page = Page}, 1, 10, 80);
	else
		-- i really should do this a better way.
		Return = HTTP.Submit(API.Post, {page = "dapi", s = "post", q = "index", tags = Tags, limit = Limit, pid = Page}, 1, 10, 80);
	end
	
	if (Return and Application.GetLastError() == 0) then
		Paragraph.SetText("Paragraph1", "Query executed successfully.");
		
		if (API.Type == "JSON") then
			local Success, Posts = pcall(JSON.Decode, Return);
			
			if (Posts and Success) then
				if (Posts.success and Posts.success == false) then
					return false, "API returned error.";
				else
					Paragraph.SetText("Paragraph1", "Decoded server response successfully.");
					
					local ReturnTable = {};
					local CensoredReturnTable = {};
					local Count = tostring(Table.Count(Posts));
					
					for K, V in pairs(Posts) do
						Paragraph.SetText("Paragraph1", "Parsing item "..K.."/"..Count.."..");
						-- TODO: V.created_at, V.change
						local Index = Table.Count(ReturnTable) + 1;
						ReturnTable[Index] = {};
						
						if (V.id) then
							ReturnTable[Index].ID = String.ToNumber(V.id);
						else
							break;
						end
						
						-- we need this. (2.60)
						ReturnTable[Index].API = API;
						
						if (V.file_size) then
							ReturnTable[Index].FormattedSize = String.GetFormattedSize(V.file_size, 1, true) or "";
							ReturnTable[Index].Size = String.ToNumber(V.file_size);
						else
							ReturnTable[Index].FormattedSize = "Unknown filesize.";
							ReturnTable[Index].Size = 0;
						end
						
						if (V.file_url) then
							ReturnTable[Index].URL = V.file_url;
						else
							ReturnTable[Index].URL = "http://danbooru.donmai.us/pagenotfound";
						end
						
						if (V.author) then
							ReturnTable[Index].Author = V.author;
						else
							ReturnTable[Index].Author = "Unknown";
						end
						
						if (V.tags) then
							ReturnTable[Index].Tags = String.Explode(" ", V.tags);
						else
							ReturnTable[Index].Tags = {};
						end
						
						if (V.creator_id) then
							ReturnTable[Index].CreatorID = String.ToNumber(V.creator_id);
						else
							ReturnTable[Index].CreatorID = 0;
						end
						
						if (V.rating) then
							ReturnTable[Index].Rating = {};
							
							ReturnTable[Index].Rating.Safe = (V.rating == "s");
							ReturnTable[Index].Rating.Questionable = (V.rating == "q");
							ReturnTable[Index].Rating.Explicit = (V.rating == "e");
							
							if (V.rating == "s") then
								ReturnTable[Index].Rating.String = "Safe";
							elseif (V.rating == "q") then
								ReturnTable[Index].Rating.String = "Questionable";
							elseif (V.rating == "e") then
								ReturnTable[Index].Rating.String = "Explicit";
							else
								ReturnTable[Index].Rating.String = "Unknown";
							end
						else
							ReturnTable[Index].Rating = {};
							
							ReturnTable[Index].Rating.Safe = false;
							ReturnTable[Index].Rating.Questionable = true;
							ReturnTable[Index].Rating.Explicit = false;
							
							ReturnTable[Index].Rating.String = "Unknown";
						end
						
						if (V.md5) then
							ReturnTable[Index].MD5 = V.md5;
						else
							ReturnTable[Index].MD5 = "d41d8cd98f00b204e9800998ecf8427e";
						end
						
						if (V.has_comments) then
							ReturnTable[Index].HasComments = (tostring(V.has_comments) == "true");
						else
							ReturnTable[Index].HasComments = false;
						end
						
						if (V.score) then
							ReturnTable[Index].Score = String.ToNumber(V.score);
						else
							ReturnTable[Index].Score = 0;
						end
						
						if (V.source) then
							ReturnTable[Index].Source = V.source;
						else
							ReturnTable[Index].Source = "http://danbooru.donmai.us/pagenotfound";
						end
						
						if (V.has_children) then
							ReturnTable[Index].HasChildren = (tostring(V.has_children) == "true");
						else
							ReturnTable[Index].HasChildren = false;
						end
						
						if (V.preview_width and V.preview_height and V.preview_url) then
							ReturnTable[Index].Preview = {};
							ReturnTable[Index].Preview.Width, ReturnTable[Index].Preview.Height, ReturnTable[Index].Preview.URL = String.ToNumber(V.preview_width), String.ToNumber(V.preview_height), V.preview_url;
						else
							ReturnTable[Index].Preview = {};
							ReturnTable[Index].Preview.Width, ReturnTable[Index].Preview.Height, ReturnTable[Index].Preview.URL = 0, 0, "http://konachan.com/images/404.jpg";
						end
						
						if (V.width and V.height) then
							ReturnTable[Index].Width, ReturnTable[Index].Height = String.ToNumber(V.width), String.ToNumber(V.height);
						else
							ReturnTable[Index].Width, ReturnTable[Index].Height = 0, 0;
						end
						
						if (V.sample_width and V.sample_height and V.sample_url) then
							ReturnTable[Index].Sample = {};
							ReturnTable[Index].Sample.Width, ReturnTable[Index].Sample.Height, ReturnTable[Index].Sample.URL = String.ToNumber(V.sample_width), String.ToNumber(V.sample_height), V.sample_url;
						else
							ReturnTable[Index].Sample = {};
							ReturnTable[Index].Sample.Width, ReturnTable[Index].Sample.Height, ReturnTable[Index].Sample.URL = 0, 0, "http://konachan.com/images/404.jpg";
						end
						
						-- TODO: Y.created_at, Y.updated_at.
						
						if (V.has_notes == true) and (tostring(Danbooru.Settings.NeverGetNotes) ~= "true") then
							ReturnTable[Index].HasNotes = true;
							
							ReturnTable[Index].Notes = {};
							
							Paragraph.SetText("Paragraph1", "Grabbing notes for post "..ReturnTable[Index].ID.."..");
							local NotesAPI = HTTP.Submit(API.Notes, {post_id = V.id}, 1, 15, 80);
							
							Paragraph.SetText("Paragraph1", "Decoding notes array..");
							local Success, Notes = pcall(JSON.Decode, NotesAPI);
							
							if (Notes and Success) then
								for X, Y in pairs(Notes) do
									local NoteIndex = Table.Count(ReturnTable[Index].Notes) + 1;
									ReturnTable[Index].Notes[NoteIndex] = {};
									
									if (Y.id) then
										ReturnTable[Index].Notes[NoteIndex].ID = String.ToNumber(Y.id);
									end
									
									if (Y.is_active) then
										ReturnTable[Index].Notes[NoteIndex].Active = (Y.is_active == "true");
									end
									
									if (Y.body) then
										ReturnTable[Index].Notes[NoteIndex].Body = Y.body;
									end
									
									if (Y.creator_id) then
										ReturnTable[Index].Notes[NoteIndex].CreatorID = String.ToNumber(Y.creator_id);
									end
									
									if (Y.width and Y.height) then
										ReturnTable[Index].Notes[NoteIndex].Width, ReturnTable[Index].Notes[NoteIndex].Height = String.ToNumber(Y.width), String.ToNumber(Y.height);
									end
									
									if (Y.x and Y.y) then
										ReturnTable[Index].Notes[NoteIndex].X, ReturnTable[Index].Notes[NoteIndex].Y = String.ToNumber(Y.x), String.ToNumber(Y.y);
									end
									
									if (Y.version) then
										ReturnTable[Index].Notes[NoteIndex].Version = String.ToNumber(Y.version);
									end
									
									Paragraph.SetText("Paragraph1", "Parsed "..X.."/"..Table.Count(Notes).." notes..");
								end
							end
						else
							ReturnTable[Index].HasNotes = false;
						end
						
						if (V.parent_id) then
							ReturnTable[Index].ParentID = String.ToNumber(V.parent_id);
						else
							ReturnTable[Index].ParentID = 0;
						end
						
						if (V.status) then
							ReturnTable[Index].Active = (V.status == "active");
							ReturnTable[Index].Pending = (V.status == "pending");
							ReturnTable[Index].Deleted = (V.status == "deleted");
							ReturnTable[Index].Status = V.status;
						else
							ReturnTable[Index].Active = false;
							ReturnTable[Index].Pending = true;
							ReturnTable[Index].Deleted = false;
							ReturnTable[Index].Status = "Unknown/Pending";
						end
	
						local Continue = true;
						
						if (Danbooru.Settings.AllowSafeImages == false) and (ReturnTable[Index].Rating.Safe) then
							Continue = false;
						end
						
						if (Danbooru.Settings.AllowQuestionableImages == false) and (ReturnTable[Index].Rating.Questionable) then
							Continue = false;
						end
						
						if (Danbooru.Settings.AllowExplicitImages == false) and (ReturnTable[Index].Rating.Explicit) then
							Continue = false;
						end	
						
						if (Danbooru.Settings.Blacklist) then
							local t_tblBlacklist = String.Explode(" ", tostring(Danbooru.Settings.Blacklist)) or {};
							local t_tblTempTags = {};
							
							for K, V in pairs(t_tblBlacklist) do
								t_tblTempTags[V] = 0;
							end
							
							for K, V in pairs(ReturnTable[Index].Tags) do
								if (t_tblTempTags[V] == 0) then
									ReturnTable[Index].Blacklisted = true;
									Continue = false;
									Danbooru.Blacklisted[ReturnTable[Index].ID] = ReturnTable[Index];
									break;
								end
							end
						end
						
						if (Continue == true) then
							table.insert(Danbooru.Posts, ReturnTable[Index]);
							table.insert(CensoredReturnTable, ReturnTable[Index]);
						end
					end
					
					return CensoredReturnTable;
				end
			else
				if (String.Find(Return, "&lt;title&gt;Downbooru&lt;/title&gt;", 1, true) ~= -1) then
					return false, "The site is down for maintenance.";
				else
					return false, "Failed to parse JSON return.";
				end
			end
		elseif (API.Type == "XML") then
			XML.SetXML(Return);
			
			if (Application.GetLastError() ~= 0) then
				Dialog.Message("Danbooru", "Error #"..Application.GetLastError().."\r\n\r\n".._tblErrorMessages[Application.GetLastError()], 0, 48, 0);
				return false, "Failed to parse XML return.";
			else
				local t_tblPosts = XML.GetElementNames("posts", true, true);
				
				local function subst(val, sub)
					val = tostring(val);
					
					if (val == "") then
						return sub;
					else
						return val;
					end
				end
				
				if (t_tblPosts) then
					local ReturnTable = {};
					local CensoredReturnTable = {};
					local Count = tostring(Table.Count(t_tblPosts));
					
					for K, V in pairs(t_tblPosts) do
						Paragraph.SetText("Paragraph1", "Parsing XML element "..K.."/"..Count.."..");
						
						local Index = (Table.Count(ReturnTable) + 1);
						
						ReturnTable[Index] = {};
						
						ReturnTable[Index].ID = String.ToNumber(subst(XML.GetAttribute(V, "id"), 0));
						ReturnTable[Index].ParentID = String.ToNumber(subst(XML.GetAttribute(V, "parent_id"), 0));
						ReturnTable[Index].CreatorID = String.ToNumber(subst(XML.GetAttribute(V, "creator_id"), 0));
						
						ReturnTable[Index].Width = String.ToNumber(subst(XML.GetAttribute(V, "width"), 0));
						ReturnTable[Index].Height = String.ToNumber(subst(XML.GetAttribute(V, "height"), 0));
						
						ReturnTable[Index].Author = subst(XML.GetAttribute(V, "author"), "Unknown");
						
						ReturnTable[Index].MD5 = subst(XML.GetAttribute(V, "md5"), "d41d8cd98f00b204e9800998ecf8427e");
						
						ReturnTable[Index].Score = subst(XML.GetAttribute(V, "score"), 0);
						
						ReturnTable[Index].URL = subst(XML.GetAttribute(V, "file_url"), 0);
						
						if (tostring(Danbooru.Settings.UseGelbooruWorkaround) == "true") then
							-- This is stupid, but I guess some people MIGHT want it.
							Paragraph.SetText("Paragraph1", "Getting post file size.. ("..K.." / "..Count..")");
							ReturnTable[Index].Size = HTTP.GetFileSize(ReturnTable[Index].URL, 0, 5, 80, nil, nil, nil);
							ReturnTable[Index].FormattedSize = String.GetFormattedSize(ReturnTable[Index].Size, 1, true);
						else
							-- No API attribute supporting this, but if it's added this will help. (3 September 2010)
							ReturnTable[Index].Size = subst(XML.GetAttribute(V, "file_size"), 0);
							
							local FormattedSize = String.GetFormattedSize(subst(XML.GetAttribute(V, "file_size"), 0), 1, true);
							
							if (FormattedSize == "0 bytes") then
								ReturnTable[Index].FormattedSize = "Unknown";
							else
								ReturnTable[Index].FormattedSize = FormattedSize;
							end
						end
						
						ReturnTable[Index].Tags = {};
						
						ReturnTable[Index].API = API;
						
						for K, V in pairs(String.Explode(" ", subst(XML.GetAttribute(V, "tags"), ""))) do
							if (String.Replace(V, " ", "", false) ~= "") then
								table.insert(ReturnTable[Index].Tags, V);
							end
						end
						
						-- No API method supporting this. (1 September 2010)
						ReturnTable[Index].HasNotes = false;
						
						ReturnTable[Index].Source = subst(XML.GetAttribute(V, "source"), API.Main);
						
						ReturnTable[Index].HasChildren = (subst(XML.GetAttribute(V, "has_children"), "false") == "true");
						ReturnTable[Index].HasComments = (subst(XML.GetAttribute(V, "has_comments"), "false") == "true");
												
						local Rating = subst(XML.GetAttribute(V, "rating"), "q");
						local Status = subst(XML.GetAttribute(V, "status"), "pending");
						
						if (Rating) then
							ReturnTable[Index].Rating = {};
							
							ReturnTable[Index].Rating.Safe = (Rating == "s");
							ReturnTable[Index].Rating.Questionable = (Rating == "q");
							ReturnTable[Index].Rating.Explicit = (Rating == "e");
							
							if (Rating == "s") then
								ReturnTable[Index].Rating.String = "Safe";
							elseif (Rating == "q") then
								ReturnTable[Index].Rating.String = "Questionable";
							elseif (Rating == "e") then
								ReturnTable[Index].Rating.String = "Explicit";
							else
								ReturnTable[Index].Rating.String = "Unknown";
							end
						else
							ReturnTable[Index].Rating = {};
							
							ReturnTable[Index].Rating.Safe = false;
							ReturnTable[Index].Rating.Questionable = true;
							ReturnTable[Index].Rating.Explicit = false;
							
							ReturnTable[Index].Rating.String = "Unknown";
						end
						
						if (Status) then
							ReturnTable[Index].Active = (Status == "active");
							ReturnTable[Index].Pending = (Status == "pending");
							ReturnTable[Index].Deleted = (Status == "deleted");
							ReturnTable[Index].Status = Status;
						else
							ReturnTable[Index].Active = false;
							ReturnTable[Index].Pending = true;
							ReturnTable[Index].Deleted = false;
							ReturnTable[Index].Status = "Unknown/Pending";
						end
						
						ReturnTable[Index].Sample, ReturnTable[Index].Preview = {}, {};
						
						ReturnTable[Index].Sample.Width, ReturnTable[Index].Sample.Height = subst(XML.GetAttribute(V, "sample_width"), 0), subst(XML.GetAttribute(V, "sample_height"), 0);
						ReturnTable[Index].Sample.URL = subst(XML.GetAttribute(V, "sample_url"), "");
						
						ReturnTable[Index].Preview.Width, ReturnTable[Index].Preview.Height = subst(XML.GetAttribute(V, "preview_width"), 0), subst(XML.GetAttribute(V, "preview_height"), 0);
						ReturnTable[Index].Preview.URL = subst(XML.GetAttribute(V, "preview_url"), "");
						
						local Continue = true;
							
						if (Danbooru.Settings.AllowSafeImages == false) and (ReturnTable[Index].Rating.Safe) then
							Continue = false;
						end
						
						if (Danbooru.Settings.AllowQuestionableImages == false) and (ReturnTable[Index].Rating.Questionable) then
							Continue = false;
						end
						
						if (Danbooru.Settings.AllowExplicitImages == false) and (ReturnTable[Index].Rating.Explicit) then
							Continue = false;
						end	
						
						if (Danbooru.Settings.Blacklist) then
							local t_tblBlacklist = String.Explode(" ", tostring(Danbooru.Settings.Blacklist)) or {};
							local t_tblTempTags = {};
							
							for K, V in pairs(t_tblBlacklist) do
								t_tblTempTags[V] = 0;
							end
							
							for K, V in pairs(ReturnTable[Index].Tags) do
								if (t_tblTempTags[V] == 0) then
									Continue = false;
									ReturnTable[Index].Blacklisted = true;
									table.insert(Danbooru.Blacklisted, ReturnTable[Index]);
									break;
								end
							end
						end
						
						if (Continue == true) then
							table.insert(Danbooru.Posts, ReturnTable[Index]);
							table.insert(CensoredReturnTable, ReturnTable[Index]);
						end
					end
					
					return CensoredReturnTable;
				else
					return false, _tblErrorMessages[Application.GetLastError()];
				end
			end
		end
	else
		return false, _tblErrorMessages[Application.GetLastError()];
	end
end


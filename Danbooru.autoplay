<Script>local Menu = {};

local function AddMenuItem(Text, Data, WorksInGelbooru)
	local Index = Table.Count(Menu) + 1;
	Menu[Index] = {Text = Text, Data = Data, ID = Index, WorksInGelbooru = WorksInGelbooru};
end

AddMenuItem("Last searched tags.", "lst:srch");
AddMenuItem("---");
AddMenuItem("Only show safe for work images.", "rating:safe");
AddMenuItem("Only show questionable images.", "rating:questionable");
AddMenuItem("Only show explicit images.", "rating:explicit");
AddMenuItem("---");
AddMenuItem("Only show posts with score above 60.", "score:&gt;60");
AddMenuItem("Only show posts from Today.", "date:"..System.GetDate(6).."-"..System.GetDate(4).."-"..System.GetDate(5), false);
AddMenuItem("---")
AddMenuItem("Sort by ID.", "order:id", false);
AddMenuItem("Sort by score.", "order:score", false);
AddMenuItem("Sort by resolution.", "order:mpixels", false);
AddMenuItem("Sort by score.", "order:score", false);
AddMenuItem("Sort by file size.", "order:filesize", false);
AddMenuItem("Sort by most popular.", "order:popular", false);
AddMenuItem("Sort by most favorited.", "order:favcount", false);
AddMenuItem("---");
AddMenuItem("Only show images equal to or higher than your display resolution.", "width:&gt;="..System.GetDisplayInfo().Width.." height:&gt;="..System.GetDisplayInfo().Height);
AddMenuItem("Only show images equal to your display resolution.", "width:"..System.GetDisplayInfo().Width.." height:"..System.GetDisplayInfo().Height);
AddMenuItem("---");
AddMenuItem("Only show active posts.", "status:active", false);
AddMenuItem("Only show flagged posts.", "status:flagged", false);
AddMenuItem("Only show pending posts.", "status:pending", false);
AddMenuItem("Only show deleted posts.", "status:deleted", false);
AddMenuItem("---");
AddMenuItem("Only show posts with two general tags.", "gentags:2", false);
AddMenuItem("Only show posts with two character tags.", "chartags:2", false);
AddMenuItem("Only show posts with two artist rags.", "arttags:2", false);
AddMenuItem("Only show posts with two copyright tags.", "copytags:2", false);

local Return = Application.ShowPopupMenu(System.GetMousePosition(false).X, System.GetMousePosition(false).Y, Menu, 0, 0, true, false);

if (Return ~= -1 and Menu[Return]) then
	if (Menu[Return].Data == "lst:srch") then
		if (tostring(Danbooru.Settings.RememberTags) == "true") then
			if (type(Danbooru.Settings.LastQuery) ~= "string") then
				Dialog.Message("Danbooru", "No data for last searched tags.", 0, 48, 0);
				return;
			else
				Input.SetText("Input1", Danbooru.Settings.LastQuery);
			end
		else
			Dialog.Message("Danbooru", "To use the last searched tags, you must first enable it in the settings dialog.", 0, 48, 0);
			return;
		end
		
		return;
	end

	if (Menu[Return].WorksInGelbooru == false and Danbooru.Sources.Gelbooru == true) then
		if (Dialog.Message("Danbooru", "This tag does not work with Gelbooru.\r\n\r\nPress 'Yes' to add the tag anyway.", 4, 32, 0) == IDYES) then
			Input.SetText("Input1", Input.GetText("Input1")..Menu[Return].Data);
		end
	else
		Input.SetText("Input1", Input.GetText("Input1")..Menu[Return].Data);
	end
end</Script>

function Danbooru.ReloadDatabase(SearchQuery, SupressDatabaseSaving)
	if not (File.DoesExist(Danbooru.LibraryPath.."\\posts.db")) then
		TextFile.WriteFromString(Danbooru.LibraryPath.."\\posts.db", JSON.Encode({}), false);
	end
	
	-- using lua io is FASTER.
	local oContents = io.open(Danbooru.LibraryPath.."\\posts.db", "r");
	
	if not (oContents) then
		return Dialog.Message("Danbooru", "Failed to load posts database.", 0, 16, 0);
	end
	
	local Contents = oContents:read("*all");
	
	oContents:close();

	Danbooru.Library = {Pages = {{}}};
	
	if (Contents ~= "") then
		local Success, Database = pcall(JSON.Decode, Contents);
		
		if (Success) then
			local CleanDatabase = {};
			local CleanerDatabase = {};
			
			for K, V in pairs(Database) do
				if (File.DoesExist(Danbooru.LibraryPath.."\\"..V.Filename)) then
					-- Should always be added.
					table.insert(CleanDatabase, V);
					
					-- Ensure that this should be added to the 'censored' table.
					if (SearchQuery) and (Table.Count(SearchQuery) ~= 0) then
						-- Should always be added.
						table.insert(CleanDatabase, V);
						
						-- Ensure that this should be added to the 'censored' table.
						if (SearchQuery) and (Table.Count(SearchQuery) ~= 0) then
							local t_tblTags, b_blnContinue = {}, true;
							
							for Index, Tag in pairs(V.Tags) do
								t_tblTags[Tag] = 0;
							end
							
							for Index, Tag in pairs(SearchQuery) do
								if not (t_tblTags[Tag]) then
									b_blnContinue = false;
								end
							end
							
							if (b_blnContinue) then
								table.insert(CleanerDatabase, V);
							end
						else
							table.insert(CleanerDatabase, V);
						end
					else
						table.insert(CleanerDatabase, V);
					end
				end
			end
			
			Danbooru.Library.Posts = CleanDatabase;
			Danbooru.Library.CensoredPosts = CleanerDatabase;
			
			Table.Sort(Danbooru.Library.Posts, nil);
			Table.Sort(Danbooru.Library.CensoredPosts, nil);
		end
		
		if not (SupressDatabaseSaving) then
			TextFile.WriteFromString(Danbooru.LibraryPath.."\\posts.db", JSON.Encode(Danbooru.Library.Posts), false);	
		end
				
		return Success;
	else
		Danbooru.Library.Posts = {};
		Danbooru.Library.CensoredPosts = {};
	end
end

function Danbooru.CommitDatabase()
	TextFile.WriteFromString(Danbooru.LibraryPath.."\\posts.db", JSON.Encode(Danbooru.Library.Posts), false);
end

-- :|
function table.copy(t)
	local t2 = {};
	
	for k,v in pairs(t) do
		t2[k] = v;
	end
	
	return t2;
end

function Danbooru.InsertPost(SourceTable, IsMultiplePosts)
	-- Lamest bug fix ever, Lua is stupid like this.
	DestinationTable = table.copy(SourceTable);
	
	Danbooru.ReloadDatabase();
	
	if (IsMultiplePosts) then
		for K, V in pairs(DestinationTable) do
			if (V and V.Preview and V.Preview.Filename) then
				table.insert(Danbooru.Library.Posts, V);
			end
		end
		
		TextFile.WriteFromString(Danbooru.LibraryPath.."\\posts.db", JSON.Encode(Danbooru.Library.Posts), false);
	else
		if (DestinationTable and DestinationTable.Preview and DestinationTable.Preview.Filename) then
			table.insert(Danbooru.Library.Posts, DestinationTable);
			TextFile.WriteFromString(Danbooru.LibraryPath.."\\posts.db", JSON.Encode(Danbooru.Library.Posts), false);
		end
	end
end

function Danbooru.SortByTags()
	-- Associative array.
	local TemporaryTags = {};
	-- Numerical array.
	local TreeTags = {};
	
	-- Post loop.
	for K, V in pairs(Danbooru.Library.Posts) do
		if (type(V.Tags) == "table") then
			for X, Y in pairs(V.Tags) do
				Y = Y:lower();
				
				if not (TemporaryTags[Y]) then
					TemporaryTags[Y] = {Count = 1, Files = {V.Filename}};
				else
					TemporaryTags[Y].Count = TemporaryTags[Y].Count + 1;
					table.insert(TemporaryTags[Y].Files, V.Filename);
				end
			end
		end
	end
	
	-- Associative to numerical.
	for K, V in pairs(TemporaryTags) do
		table.insert(TreeTags, {Tag = K, Count = V.Count, Files = V.Files});
	end
	
	-- Nullify associative and invoke GC.
	TemporaryTags = nil;
	collectgarbage();
	
	-- Return.
	return TreeTags;

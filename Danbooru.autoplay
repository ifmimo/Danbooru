<Event>
<Name>On Click</Name>
<Args/>
<Script>local Items = Danbooru.Library.Selected;

if (Items) then
	for K, V in pairs(Items) do
		if (File.DoesExist(Danbooru.LibraryPath.."\\"..V.Filename)) then
			DialogValues.SetValue("TagEditor", "DataTable", V);
			
			if (DialogEx.Show("TagEditor") == 2) then
				Page.ClickObject("xButton4");
				break;
			else
				local DataTable = DialogValues.GetValue("TagEditor", "DataTable");
				
				-- blargh.
				for K, V in pairs(Danbooru.Library.Posts) do
					if (V.Preview.Filename == DataTable.Preview.Filename) then
						Danbooru.Library.Posts[K] = DataTable;
					end
				end
			end
		end
	end
	
	Danbooru.CommitDatabase();
	Page.ClickObject("xButton4");
end</Script>

<Script>if (__CURRENT_ROW) then
	local Menu = {};
	
	Menu[1] = {Text = "Share on &amp;Facebook..", ID = 1, URL = "http://www.facebook.com/share.php?u=%s"};
	Menu[2] = {Text = "Share on &amp;Twitter..", ID = 2, URL = "http://twitter.com/home?status=%s"};
	Menu[3] = {Text = "Share on &amp;StumbleUpon..", ID = 3, URL = "http://www.stumbleupon.com/submit?url=%s"};
	Menu[4] = {Text = "---"};
	Menu[5] = {Text = "Send via &amp;Google Mail..", ID = 5, URL = "https://mail.google.com/mail/?view=cm&amp;fs=1&amp;to&amp;su="..Table.Concat(__CURRENT_ROW.Tags, " ", 1, -1).."&amp;body=%s&amp;ui=2&amp;tf=1&amp;shva=1"};
	Menu[6] = {Text = "---"};
	Menu[7] = {Text = "Shorten link with &amp;bit.ly", ID = 7, URL = "http://bit.ly/?url=%s"};
			
	local Return = Application.ShowPopupMenu(System.GetMousePosition(false).X, System.GetMousePosition(false).Y, Menu, 0, 0, true, false);
	
	if (Return ~= -1 and Menu[Return]) then	
		if (__CURRENT_ROW.API.Name == "Gelbooru") then
			File.OpenURL(Menu[Return].URL:format("http://gelbooru.com/index.php?page=post%26s=view%26id="..__CURRENT_ROW.ID));
		else
			File.OpenURL(Menu[Return].URL:format(__CURRENT_ROW.API.Main.."post/show/"..__CURRENT_ROW.ID));
		end
	end
end</Script>

<Name>On Click</Name>
<Args/>
<Script>-- Cancel the download straight away.
if (xButton.GetText(this) == "Cancel") then
	-- Next time OnProgress is called, the download will stop.
	__INNER_HTTPACTION = false;
	
	-- Stop.
	Application.ExitScript();
end

-- Get a table containing the checked rows.
local t_tblChecked = Danbooru.ListIcon:GetCheckedRows();

-- Check if the function succeeded.
if (t_tblChecked) then
	-- Define a source and destination array.
	local t_tblTargets, t_tblSuccess = {}, {};
	
	-- Start transversing the table.
	for K, V in pairs(t_tblChecked) do
		-- Confirm the data table exists for this row.
		local i_intRowID, t_tblDataTable = (V + 1), {};
		
		if (Danbooru.Posts[i_intRowID]) then
			-- Set the row data table.
			t_tblDataTable = Danbooru.Posts[i_intRowID];
			
			-- Attempt to get the files extension using fancy string work.
			local i_intExtension = String.ReverseFind(t_tblDataTable.URL, ".", false);
			local s_strExtension = ".jpg";
			
			if (i_intExtension ~= -1) then
				s_strExtension = t_tblDataTable.URL:sub(i_intExtension, -1);
			end
			
			-- Decide on a destination filename.
			local s_strDestination = Danbooru.LibraryPath.."\\"..t_tblDataTable.MD5..s_strExtension;
			
			-- Confirm the file doesn't already exist.
			if (File.DoesExist(s_strDestination)) then
				if (tostring(Danbooru.Settings.NoReplacePrompt) == "true") then
					local Keep = (tostring(Danbooru.Settings.NoReplaceType) == "keep");
					
					if not (Keep) then
						-- Populate the target table.
						table.insert(t_tblTargets, {URL = t_tblDataTable.URL, Destination = s_strDestination, DataTable = t_tblDataTable});
					end
				else
					if (tostring(Danbooru.Settings.InteractionPlaySound) == "true") then
						Audio.Load(CHANNEL_BACKGROUND, "AutoPlay\\Audio\\mp_killstrk_nuclearstrike.wav", true, false);
					end
					
					Application.Sleep(150);
					
					if (Dialog.Message("Downloading: "..Table.Concat(t_tblDataTable.Tags, " ", 1, -1):sub(1, 100).."..", "This file already exists in your downloaded items folder.\r\n\r\nWould you like to download it again anyway?", MB_YESNO, 32, MB_DEFBUTTON2) == IDYES) then
						-- Populate the target table.
						table.insert(t_tblTargets, {URL = t_tblDataTable.URL, Destination = s_strDestination, DataTable = t_tblDataTable});
					end
				end
			else
				-- Populate the target table.
				table.insert(t_tblTargets, {URL = t_tblDataTable.URL, Destination = s_strDestination, DataTable = t_tblDataTable});
			end
		end
	end
	
	-- Define download global variables.
	Danbooru.Downloads.URLs = t_tblTargets;
	Danbooru.Downloads.Index = 1;
	
	-- Define callback functions.
	function Download.OnComplete(tblData)
		-- Check if the user canceled.
		if (__DOWNLOAD == false) then
			-- Reset button to 'Download' state.
			xButton.SetImage(this, "AutoPlay\\Images\\disk.png");
			xButton.SetText(this, "Download");
			xButton.SetVisible(this, false);
			
			-- Hide the download progress bar again.
			Progress.SetVisible("Progress1", false);
			
			-- Uncheck all the checked rows.
			Danbooru.ListIcon.AllowPerformHook = false;
			Danbooru.ListIcon:CheckAllRows(false);
			Danbooru.ListIcon.AllowPerformHook = true;
			
			-- Update the download queue.
			Danbooru.UpdateDownloadQueue();
			
			-- Start scripted thumbnail generation.
			DialogValues.SetValue("Scriptify", "Files", t_tblSuccess);
			DialogEx.Show("Scriptify");
			
			-- Remove the current download from the queue.
			Download.Delete(hSession);
			
			-- Set it again. (Why?!)
			__DOWNLOAD = false;
		else
			-- Data to session.
			local hSession = tblData.Session;
			
			-- Insert this post into the success array. (for thumbnail generation)
			table.insert(t_tblSuccess, Danbooru.Downloads.URLs[Danbooru.Downloads.Index].Destination or "");
			
			-- Add filenames for local library.
			local s_strDestination = Danbooru.Downloads.URLs[Danbooru.Downloads.Index].Destination;
			local t_tblDataTable = Danbooru.Downloads.URLs[Danbooru.Downloads.Index].DataTable;
			
			t_tblDataTable.Filename = String.SplitPath(s_strDestination).Filename..String.SplitPath(s_strDestination).Extension;
			t_tblDataTable.Preview.Filename = String.SplitPath(s_strDestination).Filename..".png";
			
			-- Insert post into 'My Library'.
			Danbooru.InsertPost(t_tblDataTable);
			
			-- Start downloading the next file.
			if (Danbooru.Downloads.Index &lt; Table.Count(Danbooru.Downloads.URLs)) then
				-- Increment the download index.
				Danbooru.Downloads.Index = Danbooru.Downloads.Index + 1;
				
				-- Get the new source and destination.
				local t_tblTarget = Danbooru.Downloads.URLs[Danbooru.Downloads.Index] or {};
				
				-- Start the next download.
				Danbooru.Downloads.SessionID = Download.Start(Application.GetWndHandle(), t_tblTarget.URL or "", t_tblTarget.Destination or "", Danbooru.UserAgent, "", false, nil, 0, "", "");
				__DOWNLOAD = true;
				
				-- Remove the current download from the queue.
				Download.Delete(hSession);
			else
				-- Reset button to 'Download' state.
				xButton.SetImage(this, "AutoPlay\\Images\\disk.png");
				xButton.SetText(this, "Download");
				xButton.SetVisible(this, false);
				
				-- Hide the download progress bar again.
				Progress.SetVisible("Progress1", false);
				
				-- Uncheck all the checked rows.
				Danbooru.ListIcon.AllowPerformHook = false;
				Danbooru.ListIcon:CheckAllRows(false);
				Danbooru.ListIcon.AllowPerformHook = true;
				
				-- Update the download queue.
				Danbooru.UpdateDownloadQueue();
				
				-- Start scripted thumbnail generation.
				DialogValues.SetValue("Scriptify", "Files", t_tblSuccess);
				DialogEx.Show("Scriptify");
				
				-- Remove the current download from the queue.
				Download.Delete(hSession);
				
				-- Download queue has finished processing.
				__DOWNLOAD = false;
			end
		end
	end
	
	function Download.OnProgress(tblData)
		local hDownloadSession = tblData.Session;
		local nTotal = tblData.Total;
		local nLoaded = tblData.Loaded;
		local nTimeElapsed = tblData.TimeElapsed;
		local nCurrentSpeed = tblData.CurrentSpeed;
		local nPercent = tblData.Percent;

		if (__INNER_HTTPACTION == false) then
			-- User ended queue processing.
			__DOWNLOAD = false;
			
			-- Reset button to 'Download' state.
			xButton.SetImage(this, "AutoPlay\\Images\\disk.png");
			xButton.SetText(this, "Download");
			xButton.SetVisible(this, false);
			
			-- Hide the download progress bar again.
			Progress.SetVisible("Progress1", false);
			
			-- Uncheck all the checked rows.
			Danbooru.ListIcon.AllowPerformHook = false;
			Danbooru.ListIcon:CheckAllRows(false);
			Danbooru.ListIcon.AllowPerformHook = true;
			
			-- Update the download queue.
			Danbooru.UpdateDownloadQueue();
			
			-- Stop the download.
			Download.Stop(Danbooru.Downloads.SessionID or 0);
			
			-- Start scripted thumbnail generation.
			local b_blnShouldScriptify = false;
			
			for K, V in pairs(t_tblSuccess) do
				if (File.DoesExist(V)) then
					b_blnShouldScriptify = true;
					break;
				end
			end
			
			if (b_blnShouldScriptify) then
				DialogValues.SetValue("Scriptify", "Files", t_tblSuccess);
				DialogEx.Show("Scriptify");
				__DOWNLOAD = false;
			end
			
			-- Delete the current file.
			if (Danbooru.Downloads.URLs[Danbooru.Downloads.Index]) then
				File.Delete(Danbooru.Downloads.URLs[Danbooru.Downloads.Index].Destination, false, true, false, nil);
			end
		else
			-- Still going!
			__DOWNLOAD = true;
			
			-- Format sizes from bytes to the appropriate format.
			sDownloaded = String.GetFormattedSize((nLoaded), FMTSIZE_AUTOMATIC, true);
	 		sTotal = String.GetFormattedSize((nTotal), FMTSIZE_AUTOMATIC, true);
	 		
	 		-- Set text in progress object.
			Progress.SetText("Progress1", sDownloaded.." / "..sTotal.." ("..Math.Ceil(nCurrentSpeed).." kb/s)");
			Progress.SetCurrentPos("Progress1", ((nLoaded) / (nTotal)) * 65534);
		end
	end
	
	function Download.OnError(tblData)
		Download.Delete(tblData.Session);
		
		Dialog.Message("Danbooru", "HTTP Error: "..tostring(tblData.ErrorCode).." ("..tostring(tblData.Session).."/"..tostring(tblData.ID)..")\r\n\r\nPlease report this error to either amy.prismriver@gmail.com or blackhawk.delta@gmail.com.", 0, 16, 0);
		
		--__DOWNLOAD = false; (removing this to see if it keeps downloading images) ~Amy
	end
	
	-- Start processing the queue.
	if (Danbooru.Downloads.URLs[1]) then
		-- Show the progress bar.
		Progress.SetVisible("Progress1", true);
		
		-- Set button to 'Cancel' state.
		xButton.SetImage(this, "AutoPlay\\Images\\cross.png");
		xButton.SetText(this, "Cancel");
		
		-- Ensure the download switch is initially enabled.
		__INNER_HTTPACTION = true;
		__DOWNLOAD = true;
			
		-- Start the first download.
		Danbooru.Downloads.SessionID = Download.Start(Application.GetWndHandle(), Danbooru.Downloads.URLs[1].URL, Danbooru.Downloads.URLs[1].Destination, Danbooru.UserAgent, "", false, nil, 0, "", "");
	end
end</Script>

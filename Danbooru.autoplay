<Event>
<Name>On Show</Name>
<Args/>
<Script>local hWnd = ListBox.GetProperties("FolderView1").WindowHandle;

if (not hWnd) or (hWnd == 0) then
	Dialog.Message("Danbooru", "Failed to get object window handle, please restart application.", 0, 48, 0);
	Window.Close(Application.GetWndHandle(), 1);
end

Danbooru.LibraryListIcon = ListIconEx.CreateObject("FolderView1", "Tags", 500, {GridLines = false});
Danbooru.LibraryListIcon:SetView(LISTICON_LARGEICON);

function Danbooru.LibraryListIcon.OnDoubleClick(Row)
	if (Danbooru.LibraryListIcon.CurrentDirectory == "") then
		-- We're looking for tags.
		local str_sTag = Danbooru.LibraryListIcon:GetCellText(Row, 0);
		
		for K, V in pairs(Danbooru.LibraryListIcon.Tags) do
			if (V.Tag == str_sTag) then
				Danbooru.LibraryListIcon.CurrentDirectory = str_sTag;
				
				_SELECTED = Danbooru.LibraryListIcon:GetSelectedRows()[1];
				
				local Icon = ListIcon.LoadImage(LISTICON_LOADIMAGE_FOLDER, Danbooru.LibraryPath);
				
				Danbooru.LibraryListIcon:ClearRows();
				Danbooru.LibraryListIcon:AddRow("..", Icon);
				
				ListIcon.FreeImage(Icon);
				
				for K, V in pairs(V.Files) do
					local Icon;
					
					local ExtensionPosition, VwoExtension = String.ReverseFind(V, ".", false), V;
					
					if (ExtensionPosition ~= -1) then
						VwoExtension = V:sub(0, ExtensionPosition).."png";
					end
					
					if (File.DoesExist(Danbooru.LibraryPath.."\\.thumbnails\\"..VwoExtension)) then
						Icon = ListIcon.LoadImage(6, Danbooru.LibraryPath.."\\.thumbnails\\"..VwoExtension);
						
						if (Icon == 0) then
							Icon = ListIcon.LoadImage(5, "png");
						end
					else
						Icon = ListIcon.LoadImage(5, "png");
					end
					
					Danbooru.LibraryListIcon:AddRow(V, Icon);
					
					ListIcon.FreeImage(Icon);
				end
				
				break;
			end
		end
	else
		-- We're looking for files (and the up folder).
		local str_sName = Danbooru.LibraryListIcon:GetCellText(Row, 0);
		
		if (str_sName == "..") then
			Page.ClickObject("xButton4");
		else
			if (File.DoesExist(Danbooru.LibraryPath.."\\"..str_sName)) then
				Shell.Execute(Danbooru.LibraryPath.."\\"..str_sName, "open");
			end
		end
	end
	
	Paragraph.SetText("Paragraph2", "/"..Danbooru.LibraryListIcon.CurrentDirectory.." - "..Table.Count(Danbooru.LibraryListIcon.Tags).." tags in local database.");
end

Danbooru.LibraryListIcon:ClearRows();

Page.ClickObject("xButton4");</Script>

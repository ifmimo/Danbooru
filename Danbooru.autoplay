<Event>
<Name>On Show</Name>
<Args/>
<Script>local Data = DialogValues.GetValue("Preview", "Data");

Window.SetText(DialogEx.GetWndHandle(), "Viewing image.. (resizing is disabled when notes is enabled)");

if (Data) then
	local NotesAsString = "&lt;div id=\"note-container\" style=\"display: block;\"&gt;\r\n";
	
	if (Data.HasNotes) then
		for K, V in pairs(Data.Notes or {}) do
			NotesAsString = NotesAsString..[[&lt;div id="note-]]..V.ID..[[" style="position: absolute; background-color: #fffbb7; border: 1px #867e00 solid; font-family: Tahoma; font-size: 10pt; width: ]]..V.Width..[[px; height: ]]..V.Height..[[px; top: ]]..V.Y..[[px; left: ]]..V.X..[[px;"&gt;]]..V.Body..[[&lt;/div&gt;]];
		end
		
		NotesAsString = NotesAsString.."\r\n&lt;/div&gt;";
		
		local ImageURL;
		
		-- Data.Filename only exists when the file has been downloaded.
		if not (Data.Filename) then
			ImageURL = Data.URL;
		else
			ImageURL = "file://"..Danbooru.LibraryPath.."\\"..Data.Filename;
		end
		
		local HTMLCode = [[&lt;html&gt;
		&lt;head&gt;
			&lt;style type="text/css"&gt;
				* { padding: 0; margin: 0; }
				body { background: #000000; }
				img { vertical-align: middle; }
			&lt;/style&gt;
			&lt;script type="text/javascript"&gt;
			&lt;!--
			    function toggle_visibility(id) {
			       var e = document.getElementById(id);
			       if(e.style.display == 'block')
			          e.style.display = 'none';
			       else
			          e.style.display = 'block';
			    }
			//--&gt;
			&lt;/script&gt;
		&lt;/head&gt;
		&lt;body&gt;
			&lt;div id="content"&gt;
				]]..NotesAsString..[[
				&lt;a href="#" onclick="toggle_visibility('note-container')"&gt;&lt;img src="]]..ImageURL..[[" border="0" id="img1" /&gt;&lt;/a&gt;
			&lt;/div&gt;
		&lt;/body&gt;
	&lt;/html&gt;]];
		Web.LoadHTML("Web1", HTMLCode);
	else
		Dialog.Message("Danbooru", "This post appears to have no posts.", 0, 48, 0);
		DialogEx.Close(0);
	end
end</Script>

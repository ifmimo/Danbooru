<Script>-- Get the ListIcon host objects hWnd.
local hWnd = ListBox.GetProperties("ListBox1").WindowHandle;

-- Confirm the hWnd was found.
if (not hWnd) or (hWnd == 0) then
	Dialog.Message("Danbooru", "Failed to get object window handle, please restart application.", 0, 48, 0);
	Window.Close(Application.GetWndHandle(), 1);
end

-- Create the ListIcon object using the object wrapper class ListIconEx.
Danbooru.ListIcon = ListIconEx.CreateObject("ListBox1", "Post ID", 85, ListIconEx.GenerateFlagsTable(true, false, true, true, false, true));

-- Create the 'OnSelect' Hook.
function Danbooru.ListIcon.OnSelect(Row, Checked)
	-- Confirm a row was selected.
	if (Row == -1) then
		-- No row selected, confirm that no row was already selected.
		if (Label.GetText("Label5") ~= "No image selected.") then
			-- A row was selected previously, returning to default state.
			Web.SetVisible("PreviewWeb1", false);
			Image.Load("Image5", "AutoPlay\\Images\\img.png");
			Image.SetVisible("Image10", false);
			Image.SetVisible("Image12", false);
			Label.SetText("Label5", "No image selected.");
			Label.SetText("Label9", "Unknown");
			Label.SetText("Label10", "Unknown");
			Label.SetText("Label11", "Unknown");
			Label.SetText("Label17", "Unknown");
			Label.SetText("Label15", "Unknown");
			Label.SetText("Label16", "Unknown");
		end
		
		-- Finish.
		return;
	end
	
	-- Check to see if hooks are being allowed to perform.
	if (Danbooru.ListIcon.AllowPerformHook == false) then
		return;
	end
	
	-- Check to see if this hook is actually OnChecked.
	if (Row and Checked) then
		-- It is .OnChecked.
		Danbooru.ListIcon:SetRowSelected(Row, Checked);
	end
	
	-- Update the download queue counter.
	Danbooru.UpdateDownloadQueue();
	
	-- Get all checked rows.
	local CheckedRows = Danbooru.ListIcon:GetCheckedRows() or {};
	
	-- Only show the Download button if rows are checked.
	xButton.SetVisible("xButton4", (#CheckedRows &gt;= 1));

	-- Increment row by one.	
	local Row = Row + 1;
	
	-- Turn row into data table.
	__CURRENT_ROW = Danbooru.Posts[Row];
	
	-- Danbooru-styled color borders.
	local HTMLColor = "272727";
	
	-- Deleted: #FF0000.
	if (Danbooru.Posts[Row].Deleted) then
		HTMLColor = "FF0000";
	end
	
	-- Pending: #0000FF.
	if (Danbooru.Posts[Row].Pending) then
		HTMLColor = "0000FF";
	end
	
	-- Has child posts: #009900.
	if (Danbooru.Posts[Row].HasChildren) then
		HTMLColor = "009900";
	end
	
	-- Has parent post. #ABA100.
	if (Danbooru.Posts[Row].ParentID ~= 0) then
		HTMLColor = "ABA100";
	end
	
	-- Show the preview object.
	Web.SetVisible("PreviewWeb1", true);
	
	-- Show an image in the preview object by loading HTML into it.
	Web.LoadHTML("PreviewWeb1", [[&lt;html&gt;&lt;head&gt;&lt;style type="text/css"&gt;* { padding:0; margin:0;}
	body {background: url(']].._SFXFolder.."\\AutoPlay\\Images\\loading.png"..[[') #000;}&lt;/style&gt;&lt;/head&gt;&lt;body&gt;&lt;img style="border: 1px #]]..HTMLColor..[[ solid;" src="]]..Danbooru.Posts[Row].Preview.URL..[[" width="98" height="98"&gt;&lt;/html&gt;]], false);
	
	-- Load post information into label objects.
	Label.SetText("Label5", Table.Concat(Danbooru.Posts[Row].Tags or {}, " ", 1, -1));
	Label.SetText("Label9", (Danbooru.Posts[Row].Width or "Unknown").."x"..(Danbooru.Posts[Row].Height or "Unknown"));
	Label.SetText("Label10", Danbooru.Posts[Row].FormattedSize or "Unknown");
	Label.SetText("Label11", Danbooru.Posts[Row].Rating.String or "Unknown");
	Label.SetText("Label17", Danbooru.Posts[Row].Author or "Unknown");
	Label.SetText("Label15", Danbooru.Posts[Row].Status or "Unknown");
	Label.SetText("Label16", Danbooru.Posts[Row].Score or "Unknown");
	
	-- Show the link button next to the preview frame.
	Image.SetVisible("Image10", true);
	
	-- Show the 'social' share button.
	xButton.SetVisible("xButton5", true);
	
	-- Gelbooru workaround for getting full post URLs.
	if (Danbooru.Posts[Row].API.Name == "Gelbooru") then
		Page.SetObjectScript("Image10", "On Click", [[File.OpenURL("http://gelbooru.com/index.php?page=post&amp;s=view&amp;id=]]..Danbooru.Posts[Row].ID..[[");]]);
	else
		Page.SetObjectScript("Image10", "On Click", [[File.OpenURL("]]..Danbooru.Posts[Row].API.Main..[[post/show/]]..Danbooru.Posts[Row].ID..[[/");]]);
	end
	
	-- Set the data table to the preview table.
	DialogValues.SetValue("Preview", "Data", Danbooru.Posts[Row]);
	
	-- Show the notes button.
	if (Danbooru.Posts[Row].HasNotes) then
		Image.SetVisible("Image12", true);
	else
		Image.SetVisible("Image12", false);
	end
end

-- Both hooks are basically the same anyway..
Danbooru.ListIcon.OnChecked = Danbooru.ListIcon.OnSelect;

-- Create the 'OnRightClick' Hook.
function Danbooru.ListIcon.OnRightClick()
	-- Check if the hook is allowed to perform.
	if (Danbooru.ListIcon.AllowPerformHook == false) then
		-- Finish.
		return;
	end
	
	-- Get selected row.
	local Selected = Danbooru.ListIcon:GetSelected();
	
	-- Do nothing if nothing is selected.
	if (Selected == -1) then
		return;
	end
	
	-- Turn Row number into Data Table.
	local RowArray = Danbooru.Posts[Selected + 1] or {};
	
	-- Check if data exists.
	if (RowArray) then
		-- Get mouse position.
		local Mouse = System.GetMousePosition(false);
		
		-- Construct a menu. (can't be bothered to comment this.)
		local Menu = {};
		Menu[1] = {Text = "ID: #"..RowArray.ID.." ("..RowArray.Width.."x"..RowArray.Height.." - "..RowArray.FormattedSize..")", ID = 0, Enabled = false};
		Menu[2] = {Text = "Posted by "..RowArray.Author.."..", ID = 0, Enabled = false};
		Menu[3] = {Text = "("..RowArray.Rating.String.." - Score: "..RowArray.Score.." - Status: "..RowArray.Status..")", ID = 0, Enabled = false};
		Menu[4] = {Text = "---", Enabled = false};
		Menu[5] = {Text = "&amp;Tags..", ID = 1};
		Menu[5].SubMenu = {};
		
		for K, V in pairs((RowArray.Tags or {})) do
			table.insert(Menu[5].SubMenu, {Text = V, ID = 100 + K, Enabled = true});
		end
		
		Menu[6] = {Text = "---", Enabled = false};
		Menu[7] = {Text = "&amp;Add to download queue..", ID = 2};
		Menu[8] = {Text = "&amp;Share this image..", ID = 3, SubMenu = {}};
		Menu[8].SubMenu[1] = {Text = "Share on &amp;Facebook..", ID = 201, URL = "http://www.facebook.com/share.php?u=%s"};
		Menu[8].SubMenu[2] = {Text = "Share on &amp;Twitter..", ID = 202, URL = "http://twitter.com/home?status=%s"};
		Menu[8].SubMenu[3] = {Text = "Share on &amp;StumbleUpon..", ID = 203, URL = "http://www.stumbleupon.com/submit?url=%s"};
		Menu[8].SubMenu[4] = {Text = "---"};
		Menu[8].SubMenu[5] = {Text = "Send via &amp;Google Mail..", ID = 205, URL = "https://mail.google.com/mail/?view=cm&amp;fs=1&amp;to&amp;su="..Table.Concat(__CURRENT_ROW.Tags, " ", 1, -1).."&amp;body=%s&amp;ui=2&amp;tf=1&amp;shva=1"};
		Menu[8].SubMenu[6] = {Text = "---"};
		Menu[8].SubMenu[7] = {Text = "Shorten link with &amp;bit.ly", ID = 207, URL = "http://bit.ly/?url=%s"};
		Menu[9] = {Text = "---", Enabled = false};
		Menu[10] = {Text = "&amp;Find similar posts using IQDB..", ID = 4};
		Menu[11] = {Text = "&amp;Who favorited this post?", ID = 5};
				
		local Return = Application.ShowPopupMenu(Mouse.X, Mouse.Y, Menu, 0, 0, true, false);
		
		if (Return &gt; 100 and Return &lt; 200) then
			local ID = Return - 100;
			
			if (RowArray.Tags[ID]) then
				if (Input.GetText("Input1") == "") then
					Input.SetText("Input1", RowArray.Tags[ID]);
				else
					Input.SetText("Input1", Input.GetText("Input1").." "..RowArray.Tags[ID]);
				end
			end
		elseif (Return &gt; 200) then
			local ID = Return - 200;
			File.OpenURL(Menu[8].SubMenu[ID].URL:format(RowArray.API.Main.."post/show/"..RowArray.ID));
		elseif (Return == 2) then
			Danbooru.ListIcon:SetRowChecked(Selected, true);
		elseif (Return == 3) then
			Page.ClickObject("xButton5");
		elseif (Return == 4) then
			File.OpenURL("http://danbooru.iqdb.hanyuu.net/db-search.php?url="..RowArray.Preview.URL);
		elseif (Return == 5) then
			local Array = Danbooru.GetFavoritedByID(RowArray.ID, RowArray.API);
			
			if (type(Array) == "table") then
				local UserArray = {};
				
				for K, V in pairs(Array) do
					table.insert(UserArray, {ID = K, Text = tostring(V)});
				end
				
				if (#UserArray == 1 and UserArray[1].Text == "") then
					return Dialog.Message("Danbooru", "It appears that noone has favorited this post yet.", 0, 64, 0);
				end
				
				local UserID = Application.ShowPopupMenu(Mouse.X, Mouse.Y, UserArray, 0, 0, true, false);
				
				if (UserID ~= -1) then
					if (Array[UserID]) then
						if (Input.GetText("Input1") == "") then
							Input.SetText("Input1", "fav:"..Array[UserID]);
						else
							Input.SetText("Input1", Input.GetText("Input1").." fav:"..Array[UserID]);
						end
					end
				end
			else
				return Dialog.Message("Danbooru", "Failed to execute call to "..RowArray.API.Type.." API.");
			end
		end
	end
end

-- Called when the ListIcon wants to know what color to set the row.
function Danbooru.ListIcon.OnColorRequest(Row, Column)
	-- Row to Data table.
	local Row = (Row + 1);
	
	-- Default color table.
	local Default = {Back = 16777215, Text = 0};
	
	-- Check if the color setting is enabled.
	if (tostring(Danbooru.Settings.EnableNewColorRequest) == "true") then
		-- Check if post exists.
		if (Danbooru.Posts[Row]) then
			if (Danbooru.Posts[Row].Blacklisted) then
				-- blackisted: grey, black.
				return {Back = 14540253, Text = 0};
			elseif (Danbooru.Posts[Row].Rating.Safe) then
				-- safe: green, black.
				return {Back = 13369306, Text = 0};
			elseif (Danbooru.Posts[Row].Rating.Questionable) then
				-- questionable: yellow, black.
				return {Back = 13364479, Text = 0};
			elseif (Danbooru.Posts[Row].Rating.Explicit) then
				-- explicit: red, black.
				return {Back = 13356031, Text = 0};
			end
		else
			-- Failed to get post.
			return Default;
		end
	elseif (tostring(Danbooru.Settings.EnableNewStatusColor) == "true") then
		if (Danbooru.Posts[Row]) then
			if (Danbooru.Posts[Row].Blacklisted) then
				-- blackisted: grey, black.
				return {Back = 14540253, Text = 0};
			elseif (Danbooru.Posts[Row].Active) then
				-- active: green, black.
				return {Back = 13369306, Text = 0};
			elseif (Danbooru.Posts[Row].Pending) then
				-- pending: yellow, black.
				return {Back = 13364479, Text = 0};
			elseif (Danbooru.Posts[Row].Deleted) then
				-- active: green, black.
				return {Back = 13356031, Text = 0};
			else
				-- Status mode not implemented or malformed API response.
				return Default;
			end
		else
			-- Failed to get post.
			return Default;
		end
	else
		-- Color setting not enabled, set it to default black on white.
		return Default;
	end
end

-- Add 'Tags', 'Dimensions', 'File size' and 'Rating' columns.
Danbooru.ListIcon:AddColumn("Tags", 400);
Danbooru.ListIcon:AddColumn("Dimensions", 85);
Danbooru.ListIcon:AddColumn("File size", 75);
Danbooru.ListIcon:AddColumn("Rating", 90);

-- pool listing from v2.00.
if (type(PoolData) == "table") then
	-- Feels like an automation job.
	local Imageboard = PoolData.Imageboard;
	local ID = PoolData.ID;
	
	Input.SetText("Input1", "pool:"..ID);
	local Query = {"pool:"..ID, String.ToNumber(Input.GetText("Input3")), String.ToNumber(Input.GetText("Input2"))};
	
	for K, V in pairs(Danbooru.Sources) do
		if ((K == Imageboard) and (Danbooru.API[K]) and (Danbooru.Icons[K])) then
			Danbooru.QueryAndInsert(Danbooru.API[K], Danbooru.Icons[K], Query, Danbooru.API[K].MaxTags);
			break;
		end
	end
	
	local function s(n) if (n == 1) then return "" else return "s" end end
	
	local i_intCount = Danbooru.ListIcon:CountRows();
	
	Paragraph.SetText("Paragraph2", "("..tostring(i_intCount).." item"..s(i_intCount)..", "..Table.Count(Danbooru.Blacklisted).." blacklisted)");
	Paragraph.SetText("Paragraph1", "Idle.");
	
	PoolData = nil;
end

-- If we're an administrator, register the URL protocol here.
if not (Danbooru.PerformedProtocol) and ((System.GetUserInfo().IsAdmin or System.GetUserInfo().IsVistaAdminLimitedToken) and (tostring(Danbooru.Settings.UseURLHandler) == "true")) then
	-- Register protocol.
	Registry.SetValue(HKEY_CLASSES_ROOT, "danbooru", "", "URL:Danbooru Protocol", REG_SZ);
	Registry.SetValue(HKEY_CLASSES_ROOT, "danbooru", "URL Protocol", "", REG_SZ);
	Registry.SetValue(HKEY_CLASSES_ROOT, "danbooru\\DefaultIcon", "", "\"".._SourceFilename.."\,0", REG_SZ);
	Registry.CreateKey(HKEY_CLASSES_ROOT, "danbooru\\shell");
	Registry.CreateKey(HKEY_CLASSES_ROOT, "danbooru\\shell\\open");
	Registry.SetValue(HKEY_CLASSES_ROOT, "danbooru\\shell\\open\\command", "", "\"".._SourceFilename.."\" --url \"%1\"", REG_SZ);
	
	-- Check command-line arguments.
	for K, V in pairs(_CommandLineArgs) do
		-- Argument will be --url.
		if (V == "--url") then
			-- Next argument will be the URL protocol.
			if (_CommandLineArgs[K + 1]) then
				-- Show progress.
				Paragraph.SetText("Paragraph1", "Parsing URL protocol arguments..");
				
				-- Split the string up and work with it.
				local t_tblTempSearchQuery = String.Explode("/", _CommandLineArgs[K + 1]);
				
				-- Define the target imageboard.
				local s_strTargetImageboard;
				local t_tblTargetImageboard;
				
				-- Define the search query.
				local t_tblSearchQuery = {Imageboard = tostring(t_tblTempSearchQuery[3]), Query = Table.Concat(t_tblTempSearchQuery, " ", 4, -1)};
				
				-- Search for the target imageboard.
				for K, V in pairs(Danbooru.API) do
					if (tostring(K):lower() == t_tblSearchQuery.Imageboard:lower()) then
						-- We need the key and value for QueryAndInsert.
						s_strTargetImageboard, t_tblTargetImageboard = K, V;
						
						-- Match, stop looking.
						break;
					end
				end
				
				-- Perform search.
				if (t_tblTargetImageboard) then
					-- Construct final query table.
					local t_tblQuery = {t_tblSearchQuery.Query, (tonumber(Input.GetText("Input3") or 25)), (tonumber(Input.GetText("Input2") or 1))};
					
					-- Query /post and insert the resulting rows into the ListIcon.
					Danbooru.QueryAndInsert(t_tblTargetImageboard, Danbooru.Icons[s_strTargetImageboard], t_tblQuery, t_tblTargetImageboard.MaxTags);
					
					-- Set the tags box to show the imageboard.
					Input.SetText("Input1", t_tblSearchQuery.Query);
					
					-- Set the sources to the target imageboard only.
					Danbooru.Sources = {};

					for K, V in pairs(Danbooru.API) do
						Danbooru.Sources[K] = false;
					end
					
					Danbooru.Sources[s_strTargetImageboard] = true;
					
					Paragraph.SetText("Paragraph1", "Idle.");
					
					-- Update counters.
					local function s(n) if (n == 1) then return "" else return "s" end end
	
					local i_intCount = Danbooru.ListIcon:CountRows();
					
					Paragraph.SetText("Paragraph2", "("..tostring(i_intCount).." item"..s(i_intCount)..", "..Table.Count(Danbooru.Blacklisted).." blacklisted)");
				end
			end
		end
	end
	
	-- Finished.
	Danbooru.PerformedProtocol = true;
end</Script>

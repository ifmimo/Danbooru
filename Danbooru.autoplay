<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- CheckBox1, CheckBox2, CheckBox3, CheckBox4, CheckBox5, CheckBox6, CheckBox7, CheckBox8, CheckBox9, CheckBox10.
CheckBox.SetChecked("CheckBox1", (tostring(Danbooru.Settings.RememberSourceSelection) == "true"));
CheckBox.SetChecked("CheckBox2", (tostring(Danbooru.Settings.UseGelbooruWorkaround) == "true"));
CheckBox.SetChecked("CheckBox3", (tostring(Danbooru.Settings.ShowSplashScreen) == "true"));
CheckBox.SetChecked("CheckBox4", (tostring(Danbooru.Settings.ClearListOnNewSearch) == "true"));
CheckBox.SetChecked("CheckBox5", (tostring(Danbooru.Settings.InteractionPlaySound) == "true"));
CheckBox.SetChecked("CheckBox6", (tostring(Danbooru.Settings.NoReplacePrompt) == "true"));
CheckBox.SetChecked("CheckBox7", (tostring(Danbooru.Settings.NeverGetNotes) == "true"));
CheckBox.SetChecked("CheckBox8", (tostring(Danbooru.Settings.RememberTags) == "true"));
CheckBox.SetChecked("CheckBox9", (tostring(Danbooru.Settings.RememberPageAndLimit) == "true"));
CheckBox.SetChecked("CheckBox10", (tostring(Danbooru.Settings.UseURLHandler) == "true"));

-- ComboBox1.
local IsOverwrite = (tostring(Danbooru.Settings.NoReplaceType) == "overwrite");

if (IsOverwrite) then
	ComboBox.SetSelected("ComboBox1", 1);
else
	ComboBox.SetSelected("ComboBox1", 2);
end

-- RadioButton1, RadioButton2, RadioButton3, RadioButton4, RadioButton5.
RadioButton.SetChecked("RadioButton1", false);
RadioButton.SetChecked("RadioButton2", false);
RadioButton.SetChecked("RadioButton3", false);
RadioButton.SetChecked("RadioButton4", false);
RadioButton.SetChecked("RadioButton5", false);

if (tostring(Danbooru.Settings.AllowSafeImages) == "true") then
	RadioButton.SetChecked("RadioButton1", true);
	RadioButton.SetChecked("RadioButton2", false);
	RadioButton.SetChecked("RadioButton3", false);
end

if (tostring(Danbooru.Settings.AllowQuestionableImages) == "true") then
	RadioButton.SetChecked("RadioButton1", false);
	RadioButton.SetChecked("RadioButton2", true);
	RadioButton.SetChecked("RadioButton3", false);
end

if (tostring(Danbooru.Settings.AllowExplicitImages) == "true") then
	RadioButton.SetChecked("RadioButton1", false);
	RadioButton.SetChecked("RadioButton2", false);
	RadioButton.SetChecked("RadioButton3", true);
end

RadioButton.SetChecked("RadioButton4", (tostring(Danbooru.Settings.EnableNewColorRequest) == "true"));
RadioButton.SetChecked("RadioButton5", (tostring(Danbooru.Settings.EnableNewStatusColor) == "true"));

-- Input1.
if (Danbooru.Settings.Blacklist) then
	Input.SetText("Input2", tostring(Danbooru.Settings.Blacklist));
end</Script>

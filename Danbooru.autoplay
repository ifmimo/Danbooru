<Event>
<Name>On Click</Name>
<Args/>
<Script>local LibraryLocation = Input.GetText("Input1");

if (LibraryLocation == "") or not (Folder.DoesExist(LibraryLocation)) then
	return Dialog.Message("Danbooru", "You must choose an existing folder before continuing.", 0, 48, 0);
else
	Danbooru.SetSettingsValue("LibraryPath", LibraryLocation);
	Danbooru.LibraryPath = LibraryLocation;
end

local SFWFilter = RadioButton.GetChecked("RadioButton1");
local QSFWFilter = RadioButton.GetChecked("RadioButton2");
local NoFilter = RadioButton.GetChecked("RadioButton3");

if (SFWFilter) then
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", false);
	Danbooru.SetSettingsValue("AllowExplicitImages", false);
elseif (QSFWFilter) then
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", true);
	Danbooru.SetSettingsValue("AllowExplicitImages", false);
else
	Danbooru.SetSettingsValue("AllowSafeImages", true);
	Danbooru.SetSettingsValue("AllowQuestionableImages", true);
	Danbooru.SetSettingsValue("AllowExplicitImages", true);
end

Danbooru.SetSettingsValue("ClearListOnNewSearch", CheckBox.GetChecked("CheckBox4"));
Danbooru.SetSettingsValue("ShowSplashScreen", CheckBox.GetChecked("CheckBox2"));

Danbooru.SetSettingsValue("EnableNewColorRequest", RadioButton.GetChecked("RadioButton4"));
Danbooru.SetSettingsValue("EnableNewStatusColor", RadioButton.GetChecked("RadioButton5"));

Danbooru.SetSettingsValue("RememberSourceSelection", CheckBox.GetChecked("CheckBox1"));

Danbooru.SetSettingsValue("Blacklist", Input.GetText("Input2"));

DialogEx.Close(0);</Script>

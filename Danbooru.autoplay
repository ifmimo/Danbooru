<Script>-- Store all items in the Menu bar.
if not (StoredMenu) then
	StoredMenu = Application.GetMenu();
end

-- Used when we return from 'My Library' to restore the menu bar.
Application.SetMenu(StoredMenu);

-- If the settings file doesn't exist go through first-time setup.
if (not File.DoesExist(_SourceFolder.."\\settings.db")) then
	-- Show the first-time setup dialog.
	if (DialogEx.Show("Welcome") == 0) then
		-- If it all worked, write the settings file.
		Danbooru.WriteSettingsFile();
	else
		-- The user canceled, Terminate the program.
		Window.Close(Application.GetWndHandle(), 1);
	end
end

-- Wipe incase first launch.
Danbooru.Settings = {};

-- Decode the settings database.
local Success, Settings = pcall(JSON.Decode, TextFile.ReadToString(_SourceFolder.."\\settings.db"));

-- Set the new Danbooru.Settings table.
if (Success and type(Settings) == "table") then
	Danbooru.Settings = Settings;
end

-- Set ther library path, and if it doesn't exist we create it.
if (type(Danbooru.Settings.LibraryPath) == "string") and (Folder.DoesExist(Danbooru.Settings.LibraryPath)) then
	Danbooru.LibraryPath = Danbooru.Settings.LibraryPath;
	
	if not (Folder.DoesExist(Danbooru.LibraryPath)) then
		Folder.Create(Danbooru.LibraryPath);
	end
end

-- Set the sources selection state from the settings file.
if (type(Danbooru.Settings.Selected) == "table") then
	for K, V in pairs(Danbooru.Settings.Selected) do
		Danbooru.Sources[K] = (tostring(Danbooru.Settings.Selected[K]) == "true");
	end
end

-- Set the page and limit from the settings file.
if (tostring(Danbooru.Settings.RememberPageAndLimit) == "true") then
	Input.SetText("Input2", tonumber(Danbooru.Settings.Page) or 1);
	Input.SetText("Input3", tonumber(Danbooru.Settings.Limit) or 25);
end

-- Set the Window title.
Window.SetText(Application.GetWndHandle(), Danbooru.VersionString);

-- If using Windows XP, show a line near the top of the window frame. (not tested)
Plugin.SetVisible("Plugin4", (System.GetOSName() == "Windows XP"));

-- Empty the ListIcon.
if (Danbooru.ListIcon) then
	Danbooru.EmptyListIcon();
end

-- Show splash screen. (~Reag)
if (tostring(Danbooru.Settings.ShowSplashScreen) == "true") and (Danbooru.SplashShown == false) then
	-- I don't like this.. (~shadiku)
	local sImage = Danbooru.SplashImagePath[1];
	
	for i = 2, 15 do
		sImage = sImage..", "..Danbooru.SplashImagePath[i];
	end
	
	for i = 1, 35 do
		sImage = sImage..", "..Danbooru.SplashImagePath[15];
	end
	
	for i = 15, 1, -1 do
		sImage = sImage..", "..Danbooru.SplashImagePath[i];
	end
	
	DLL.CallFunction("AutoPlay\\Docs\\SplashImage.dll", "SplashImage", Application.GetWndHandle()..",\""..sImage.."\",2,25,1,0", DLL_RETURN_TYPE_LONG, DLL_CALL_STDCALL);
end

-- This is set here regardless. (~Reag)
Danbooru.SplashShown = true;</Script>

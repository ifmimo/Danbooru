<Event>
<Name>On Click</Name>
<Args/>
<Script>Danbooru.LibraryListIcon.CurrentDirectory = "";
Danbooru.LibraryListIcon.Tags = Danbooru.SortByTags() or {};

Danbooru.LibraryListIcon:ClearRows();

local Icon = ListIcon.LoadImage(3, Danbooru.LibraryPath);

for K, V in pairs(Danbooru.LibraryListIcon.Tags) do
	Danbooru.LibraryListIcon:AddRow(V.Tag, Icon);
end

if (_SELECTED) then
	Danbooru.LibraryListIcon:EnsureVisible(_SELECTED);
	Danbooru.LibraryListIcon:SetSelected(_SELECTED);
	_SELECTED = nil;
end

ListIcon.FreeImage(Icon);

Paragraph.SetText("Paragraph2", "/ - "..Table.Count(Danbooru.LibraryListIcon.Tags).." tags in local database.");</Script>
